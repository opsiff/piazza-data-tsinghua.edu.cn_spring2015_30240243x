<script type="text/javascript">
P.modules.loadModule("poll_results", {}, null, "\n\u003cdiv class=\"post_region_box\" id=\"poll_results\"\u003e\n{{#unless toShow.poll_unpublished}}\n  \u003cdiv class=\"post_region_content member_answer\"\u003e\n\n{{#if toShow.is_instructor}}\n\n    {{#if toShow.poll_closed}}\n    \u003ch2 id=\"poll_closed\" class=\"head show\"\u003e\u003cb\u003e{{content.subject}}\u003c/b\u003e is now closed\u003c/h2\u003e\n    {{/if}}\n\n    {{#if toShow.poll_will_close}}\n    \u003ch2 id=\"poll_will_close\" class=\"head show\"\u003e\u003cb\u003e{{content.subject}}\u003c/b\u003e closes in \u003cspan class=\"poll_closes_in\"\u003e{{toShow.poll_closes_in}}\u003c/span\u003e\u003c/h2\u003e\n    {{/if}}\n\n{{else}}\n\n    {{#if toShow.poll_submitted}}\n    \u003ch2 id=\"poll_submitted\" class=\"head show\"\u003eThanks for submitting your response!\u003c/h2\u003e\n    {{/if}}\n    {{#if toShow.poll_not_submitted}}\n    \u003ch2 id=\"poll_not_submitted\" class=\"head show\"\u003eSubmit your vote to see results\u003c/h2\u003e\n    {{/if}}\n\n    {{#if toShow.poll_closed}}\n    \u003cdiv id=\"poll_closed\" class=\"head show\"\u003e\u003cb\u003e{{content.subject}}\u003c/b\u003e is now closed\u003c/div\u003e\n    {{/if}}\n    {{#if toShow.poll_not_closed}}\n    \u003cdiv id=\"poll_not_closed\" class=\"head show\"\u003eResults are visible after the poll closes in \u003cspan class=\"poll_closes_in\"\u003e{{toShow.poll_closes_in}}\u003c/span\u003e\u003c/div\u003e\n    {{/if}}\n    {{#if toShow.poll_will_close}}\n    \u003cdiv id=\"poll_will_close\" class=\"head show\"\u003e\u003cb\u003e{{content.subject}}\u003c/b\u003e closes in \u003cspan class=\"poll_closes_in\"\u003e{{toShow.poll_closes_in}}\u003c/span\u003e\u003c/div\u003e\n    {{/if}}\n\n    {{#if toShow.poll_only_instructors}}\n    \u003cdiv id=\"poll_only_instructors\" class=\"head show\"\u003eResult is only visible to instructors\u003c/div\u003e\n    {{/if}}\n\n    {{#if toShow.poll_no_data}}\n    \u003cdiv id=\"poll_no_data\" class=\"head show\"\u003eThere are no results yet\u003c/div\u003e\n    {{/if}}\n\n{{/if}}\n\n\n\n{{#if toShow.is_instructor}}\n    {{#ifGreater content.data.total_votes 0}}\n\n    \u003cdiv class=\"head2\"\u003eA total of \u003cb\u003e\u003cspan class=\"num_poll_votes\"\u003e{{content.data.total_votes}}\u003c/span\u003e\u003c/b\u003e vote(s) in \u003cb\u003e\u003cspan class=\"poll_time\"\u003e{{toShow.poll_time}}\u003c/span\u003e\u003c/b\u003e hours\u003c/div\u003e\n    {{else}}\n        \u003cdiv class=\"head2\"\u003eA total of 0 votes in \u003cb\u003e\u003cspan class=\"poll_time\"\u003e{{toShow.poll_time}}\u003c/span\u003e\u003c/b\u003e hours\u003c/div\u003e\n    {{/ifGreater}}\n    {{#if toShow.poll_toggle_visibility_on}}\n    \u003cdiv class=\"poll_results_wrapper\"\u003e\n      {{#each results}}\n        {{#pollResult this}}{{/pollResult}}\n      {{/each}}\n    \u003c/div\u003e\n\n    {{else}}\n    \u003cdiv class=\"head show\" data-pats=\"poll_results_hidden_prof\"\u003ePoll results are hidden.  To view, click Show Poll Results in the admin panel.\u003c/div\u003e\n\n\n    {{/if}}\n\n{{else}}\n    {{#if toShow.poll_show_results}}\n    {{#ifGreater content.data.total_votes 0}}\n    \u003cdiv class=\"head2\"\u003eA total of \u003cb\u003e\u003cspan class=\"num_poll_votes\"\u003e{{content.data.total_votes}}\u003c/span\u003e\u003c/b\u003e vote(s) in \u003cb\u003e\u003cspan class=\"poll_time\"\u003e{{toShow.poll_time}}\u003c/span\u003e\u003c/b\u003e hours\u003c/div\u003e\n    {{else}}\n            \u003cdiv class=\"head2\"\u003eA total of 0 votes in \u003cb\u003e\u003cspan class=\"poll_time\"\u003e{{toShow.poll_time}}\u003c/span\u003e\u003c/b\u003e hours\u003c/div\u003e\n\n    {{/ifGreater}}\n    {{#if toShow.poll_toggle_visibility_on}}\n    \u003cdiv class=\"poll_results_wrapper\"\u003e\n      {{#each results}}\n        {{#pollResult this}}{{/pollResult}}\n      {{/each}}\n    \u003c/div\u003e\n    {{else}}\n    \u003cdiv class=\"head show\" data-pats=\"poll_results_hidden_student\"\u003ePoll results are hidden by your instructor.\u003c/div\u003e\n    {{/if}}\n    {{/if}}\n\n{{/if}}\n\n    {{#if toShow.poll_closed}}\n        {{#if explanation}}\n            {{#ifNotEquals explanation \"\"}}\n                \u003cdiv id=\"poll_closed_answer\" class=\"head show\"\u003eExplanation: \u003cb\u003e{{explanation}}\u003c/b\u003e\u003c/div\u003e\n            {{/ifNotEquals}}\n        {{/if}}\n    {{/if}}\n  \u003c/div\u003e\n\n\n{{/unless}}\n\n\u003c/div\u003e\n\n");</script>

<script type="text/javascript">
P.modules.loadModule("answer", {}, null, "\u003cdiv class=\"post_region_box answer\" id=\"{{answer.type}}\"\u003e\n  \u003cdiv class=\"post_region_header clearFix\"\u003e\n    \u003cdiv class=\"post_icon {{answer.icon}}\"\u003e\u003c/div\u003e\n    \u003cdiv class=\"post_title\"\u003e{{answer.title}},\u003c/div\u003e\n    \u003cdiv class=\"post_subtitle\"\u003e{{answer.long}}\u003c/div\u003e\n  \u003c/div\u003e\n  {{#if answer.showEdit}}\n  \u003cdiv class=\"dropdown post_region_arrow_dropdown\"\u003e\n    \u003ca href=\"#\" class=\"dropdown-toggle\" data-toggle=\"dropdown\"\u003eActions \u003cb class=\"caret\"\u003e\u003c/b\u003e\u003c/a\u003e\n    \u003cdiv class=\"dropdown_menu_wrapper clearFix\"\u003e\n      \u003cul class=\"dropdown-menu\" role=\"menu\" id=\"answer_actions_dropdown\" aria-labelledby=\"dLabel\"\u003e\n        \u003c!--\u003cli class=\"attach\"\u003e\u003ca class=\"attach\" href=\"#\"\u003eAttach File\u003c/a\u003e\u003c/li\u003e--\u003e\n        \u003cli class=\"report_spam\"\u003e\u003ca class=\"report_spam\" href=\"#\"\u003eFlag as Inappropriate\u003c/a\u003e\u003c/li\u003e\n        {{#if user.isInst}} {{#unless content.disabledHistory}}\n        \u003cli\u003e\u003ca href=\"#\" onclick=\"return P.history_slider.toggleBan();\" \u003e\u003cspan class=\"userBannerText\"\u003eOpen Ban User Console\u003c/span\u003e\u003c/a\u003e\u003c/li\u003e\n        {{/unless}}{{/if}}\n      \u003c/ul\u003e\n    \u003c/div\u003e\n  \u003c/div\u003e\n  {{/if}}\n  {{#if answer.updated}}\n    \u003cdiv class=\"post_region_content view_mode\"\u003e\n    {{#answerText answer network}}{{/answerText}}\n    \u003c/div\u003e\n  {{/if}}\n  {{#if answer.isActiveTimer}}\n    {{#unless answer.updated}}\n    \u003cdiv class=\"qa_timer_container\"\u003e\n      \u003cdiv class=\"qa_timer_option_container\"\u003e\n        Instructors are encouraging students to attempt to answer this question.\n      \u003c/div\u003e\n    \u003c/div\u003e\n    {{/unless}}\n  {{/if}}\n  {{#if answer.isInstructor}}\n    {{#if network.config.timer.enabled}}\n      {{#unless answer.updated}}\n  \u003cdiv class=\"qa_timer_container\"\u003e\n    \u003cdiv class=\"qa_timer_option_container\"\u003e\n      {{#unless content.config.timer.enabled}}\n      \u003cdiv class=\"timer_delay_icon small grey\"\u003e\u003c/div\u003e\n      \u003ca href=\"#\" onclick=\"return P.answer.setTimer(P.answer.network.config.timer.delay)\" class=\"set_timer_link\"\u003eSet timer\u003c/a\u003e\n      {{/unless}}\n      {{#if content.config.timer.delay}}\n        {{#if content.config.timer.isActive}}\n          \u003cdiv class=\"timer_delay_icon small green\"\u003e\u003c/div\u003e\n          \u003cspan class=\"timer_set_text\"\u003eTimer set\u003c/span\u003e\n          {{#if content.config.timer.user_id}}\n            \u003cspan class=\"timer_set_meta\"\u003e{{#toRelativeTime content.config.timer.when}}{{/toRelativeTime}} by {{#toUserName content.config.timer.user_id}}{{/toUserName}}\u003c/span\u003e\n          {{else}}\n            \u003cspan class=\"timer_set_meta\"\u003eDefault timer set {{#toRelativeTime content.config.timer.when}}{{/toRelativeTime}}\u003c/span\u003e\n          {{/if}}\n        {{else}}\n          \u003cdiv class=\"timer_delay_icon small red\"\u003e\u003c/div\u003e\n          \u003cspan class=\"timer_set_text\"\u003eTimer done\u003c/span\u003e\n          {{#if content.config.timer.user_id}}\n            \u003cspan class=\"timer_set_meta\"\u003e{{#toRelativeTime content.config.timer.when}}{{/toRelativeTime}} by {{#toUserName content.config.timer.user_id}}{{/toUserName}}\u003c/span\u003e\n          {{else}}\n            \u003cspan class=\"timer_set_meta\"\u003eDefault timer set {{#toRelativeTime content.config.timer.when}}{{/toRelativeTime}}\u003c/span\u003e\n          {{/if}}\n        {{/if}}\n      {{/if}}\n    \u003c/div\u003e\n    {{#if content.config.timer.enabled}}\n    \u003cdiv class=\"qa_timer_length_container\"\u003e\n      \u003cspan\u003eTimer length:\u003c/span\u003e\n      \u003clabel for=\"thirty_min\"\u003e\n        \u003cinput type=\"radio\" id=\"thirty_min\" onchange=\"P.answer.setTimer(1)\" name=\"timer_length\" {{#ifEquals content.config.timer.delay 1}}checked{{/ifEquals}}/\u003e\n        \u003cspan\u003e30 minutes\u003c/span\u003e\n      \u003c/label\u003e\n      \u003clabel for=\"two_hours\"\u003e\n        \u003cinput type=\"radio\" id=\"timer_2\" onchange=\"P.answer.setTimer(4)\" name=\"timer_length\" {{#ifEquals content.config.timer.delay 4}}checked{{/ifEquals}}/\u003e\n        \u003cspan\u003e2 hours\u003c/span\u003e\n      \u003c/label\u003e\n      \u003clabel for=\"four_hours\"\u003e\n        \u003cinput type=\"radio\" id=\"timer_4\" onchange=\"P.answer.setTimer(8)\" name=\"timer_length\" {{#ifEquals content.config.timer.delay 8}}checked{{/ifEquals}}\u003e\n        \u003cspan\u003e4 hours\u003c/span\u003e\n      \u003c/label\u003e\n      \u003clabel for=\"eight_hours\"\u003e\n        \u003cinput type=\"radio\" id=\"timer_8\" onchange=\"P.answer.setTimer(16)\" name=\"timer_length\" {{#ifEquals content.config.timer.delay 16}}checked{{/ifEquals}}\u003e\n        \u003cspan\u003e8 hours\u003c/span\u003e\n      \u003c/label\u003e\n      \u003clabel for=\"twenty_four_hours\"\u003e\n        \u003cinput type=\"radio\" id=\"timer_24\" onchange=\"P.answer.setTimer(48)\" name=\"timer_length\" {{#ifEquals content.config.timer.delay 48}}checked{{/ifEquals}}\u003e\n        \u003cspan\u003e24 hours\u003c/span\u003e\n      \u003c/label\u003e\n      \u003clabel for=\"timer_off\"\u003e\n        \u003cinput type=\"radio\" id=\"timer_off\" onchange=\"P.answer.setTimer(false)\" name=\"timer_length\" {{#ifEquals content.config.timer.delay false}}checked{{/ifEquals}}\u003e\n        \u003cspan\u003eoff\u003c/span\u003e\n      \u003c/label\u003e\n    \u003c/div\u003e\n    {{/if}}\n  \u003c/div\u003e\n      {{/unless}}\n    {{/if}}\n  {{/if}}\n  \u003cdiv class=\"post_region_content edit_mode\" id=\"edit_member_answer\" style=\"display:none;\"\u003e\n    {{#if answer.showSuper}}\n    \u003cdiv class=\"message-alert super-edit-answer\" id=\"super-answer-2\" style=\"background-color: rgb(248, 202, 202); margin-bottom: 7px; color: rgb(153, 0, 0); border: 1px solid rgb(153, 0, 0); display: block; background-position: initial initial; background-repeat: initial initial;\"\u003e\n      \u003cspan class=\"message\"\u003e\n        \u003cem\u003e!\u003c/em\u003e\u0026nbsp;\u0026nbsp;As an instructor, you're editing the Students' Response in super-user mode.\n      \u003c/span\u003e\n    \u003c/div\u003e\n    {{/if}}\n    \u003ch3\u003eEdit Answer\u003c/h3\u003e\n    \u003cdiv class=\"answer_edit_wrapper\"\u003e\u003c/div\u003e\n  \u003c/div\u003e\n  \u003cdiv class=\"post_region_message_wrapper currently_editing\" style=\"display:none;margin-top:3px;\"\u003e\n    \u003cdiv class=\"post_region_message warning show\"\u003e\n      \u003cspan class=\"editing_message\"\u003eThis is red text\u003c/span\u003e\n    \u003c/div\u003e\n  \u003c/div\u003e\n  \u003cdiv class=\"post_region_actions edit_mode\"\u003e\n    \u003ca data-pats=\"submit_button\" class=\"post_action submit main btn btn-primary btn-mini submit_post\" href=\"#\"\u003esubmit\u003c/a\u003e\n    {{#if anon}}\n       \u003cspan class=\"post_action\"\u003e\n     \u0026nbsp;\u0026nbsp;as \u003cselect class=\"anonymity_selector\" data-pats=\"anonymity_selector\"\u003e\n        {{#each anon}}\n        \u003coption value=\"{{this.val}}\"\u003e{{this.title}}\u003c/option\u003e\n        {{/each}}\n      \u003c/select\u003e\n      \u003c/span\u003e\n    {{/if}}\n    \u003ca id=\"autosave_button_{{answer.type}}\" class=\"post_action save_draft btn btn-secondary btn-mini\" href=\"#\" style=\"margin: 0;margin-left:3px;\" onclick=\"return P.text_editor.autoSave('answer_{{answer.type}}', true);\"\u003esave draft\u003c/a\u003e\n    \u003cspan class=\"middot\"\u003e\u0026middot;\u003c/span\u003e\n    \u003ca class=\"post_action preview preview_link\" id=\"preview_link_answer_{{answer.type}}\" href=\"#\" onclick=\"return P.text_editor.handlePreviewButton('answer_{{answer.type}}', true);\"\u003epreview\u003c/a\u003e\n    \u003ca class=\"post_action cancel right\" href=\"#\"\u003ecancel\u003c/a\u003e\n  \u003c/div\u003e\n  {{#if answer.updated}}\n    \u003cdiv class=\"post_region_actions view_mode\"\u003e\n      {{#if answer.showEdit}}\n      \u003ca data-pats=\"edit_button\" class=\"post_action edit main btn btn-mini btn-primary\" tutorial=\"Edit the single Wiki Answer to improve the answer.\" href=\"#\"\u003eedit\u003c/a\u003e\n      \u003cspan class=\"middot\"\u003e\u0026middot;\u003c/span\u003e\n      {{/if}}\n      {{#if endorsed}}\n        \u003ca data-pats=\"unmark_answer_good\" class=\"post_action undo_good_answer\" href=\"#\"\u003eundo {{#if user.isInst}}good answer{{else}}thanks{{/if}}\u003c/a\u003e\n        \u003ca data-pats=\"mark_answer_good\" class=\"post_action do_good_answer\" style=\"display:none;\" href=\"#\"\u003e{{#if user.isInst}}good answer{{else}}thanks!{{/if}}\u003c/a\u003e\n      {{else}}\n        \u003ca data-pats=\"unmark_answer_good\" class=\"post_action undo_good_answer\" style=\"display:none;\" href=\"#\"\u003eundo {{#if user.isInst}}good answer{{else}}thanks{{/if}}\u003c/a\u003e\n        \u003ca data-pats=\"mark_answer_good\" class=\"post_action do_good_answer\" href=\"#\"\u003e{{#if user.isInst}}good answer{{else}}thanks!{{/if}}\u003c/a\u003e\n      {{/if}}\n      \u003cspan data-pats=\"mark_answer_count\" class=\"post_actions_number good_answer\"\u003e{{answer.endorseCnt}}\u003c/span\u003e\n      \u003cdiv class=\"post_region_actions_meta\" data-pats=\"updated_text\"\u003eUpdated {{#toRelativeTime answer.updated}}{{/toRelativeTime}} by\n        {{#if answer.uidSet}}\n          {{#toUserName answer.uid answer.anon false answer.uidA}}{{/toUserName}}\n        {{else}}\n          \u003cspan class=\"privatePoster highlight\"\u003eAnonymous\u003c/span\u003e\n        {{/if}}\n        {{#showOthers answer.others answer.othersAnon answer.othersA}}{{/showOthers}}\n      \u003c/div\u003e\n    \u003c/div\u003e\n  {{else}}\n    \u003cdiv class=\"post_region_content compose_mode\"\u003e\n      \u003cdiv data-pats=\"start_textbox\" class=\"compose_answer\"\u003eClick to start off the wiki answer\u003c/div\u003e\n    \u003c/div\u003e\n  {{/if}}\n\n {{#if user.isInst}}\n {{#ifEquals answer.type \"i_answer\"}}\n {{#if network.config.show_dupes}}\n  \u003cdiv class=\"similar-posts in-answer-box\" style=\"display:none;\"\u003e\n    \u003cdiv style=\"margin-top: 10px;margin-left: 10px;font-weight: 600;\"\u003ePosts that may be similar: \u003cspan style=\"font-weight: 400\"\u003e{{#if similar}}{{#each similar}}\u003ca href=\"/class/{{network.id}}?cid={{this}}\" target=\"_blank\" style=\"color:#888\"\u003e@{{this}}\u003c/a\u003e; {{/each}}{{else}}-expand to find similar posts-{{/if}}\u003c/span\u003e\n    \u003cbr/\u003e\u003cspan style=\"font-weight: 400;font-size: 11px;color: #888;\"\u003eHave feedback on our similar post suggestions? Email us at \u003ca href=\"mailto:productfeedback@piazza.com\"\u003eproductfeedback@piazza.com\u003c/a\u003e\u003c/span\u003e\n      \u003cdiv style=\"float:right;font-weight: 400;\"\u003eDefault: \u003clabel style=\"display:inline-block;font-size:13px;\"\u003e\u003cinput type=\"radio\" name=\"similar_radil\" id=\"similar_expand\" {{#if user.config.answer_dupe}}checked{{/if}} onclick=\"P.answer.showDupes(true)\" style=\"margin-top: -3px;\"/\u003e expand\u003c/label\u003e\u0026nbsp;\u0026nbsp;\u0026nbsp;\u0026nbsp;\u003clabel style=\"display:inline-block;font-size:13px;\"\u003e\u003cinput type=\"radio\" name=\"similar_radil\" id=\"similar_collapse\" {{#unless user.config.answer_dupe}}checked{{/unless}} onclick=\"P.answer.showDupes(false)\" style=\"margin-top: -3px;\"/\u003e collapse\u003c/label\u003e\u003c/div\u003e\n    \u003c/div\u003e\n    \u003cul id=\"similar-posts\"\u003e\n    \u003c/ul\u003e\n  \u003c/div\u003e\n  {{/if}}\n  {{/ifEquals}}\n  {{/if}}\n\u003c/div\u003e\n");</script>

<script type="text/javascript">
P.modules.loadModule("clarifying_discussion", {}, null, "\u003cdiv id=\"clarifying_discussion\" class=\"post_region_box clarifying_discussion dashboard_element\"\u003e\n  \u003cdiv class=\"post_region_header clearFix\"\u003e\n    \u003cdiv class=\"post_title\"\u003efollowup discussions\u003c/div\u003e\n    \u003cdiv class=\"post_subtitle\"\u003efor lingering questions and comments\u003c/div\u003e\n    \u003cdiv class=\"pull-right num_endorsement_top\"  style=\"text-align: center; color: #008000; font-weight: bold; font-size: 11px; \"\u003e{{#numEndorsementsStr this}}{{/numEndorsementsStr}}\u003c/div\u003e\n  \u003c/div\u003e\n  {{#if content.config.resolved_followups}}\n  \u003cdiv class=\"post_region_header show_limited_posts clearFix\" id=\"resolved_followups\"\u003e\n    showing the last 30 followups:\n  \u003c/div\u003e\n  {{/if}}\n  \u003cdiv data-pats=\"followups\" class=\"post_region_content clarifying_discussion\"\u003e\n    {{#each content.followups}}\n      {{#followupMessage this}}{{/followupMessage}}\n    {{/each}}\n    \u003ch5 id=\"start_new_followup_header\"\u003eStart a new followup discussion\u003c/h5\u003e\n    \u003cdiv class=\"compose_discussion\" id=\"create_new_followup\"\u003eCompose a new followup discussion\u003c/div\u003e\n    \u003cdiv class=\"clarifying_discussion new clearFix\" id=\"create_new_followup_div\"\u003e\n      \u003cdiv class=\"account_image_container\"\u003e\n        {{#toUserPic user.id}}{{/toUserPic}}\n      \u003c/div\u003e\n      \u003cdiv class=\"discussion_content\" id=\"create_new_followup_editor\"\u003e\u003c/div\u003e\n    \u003c/div\u003e\n  \u003c/div\u003e\n\u003c/div\u003e\n");</script>

<script type="text/javascript">
P.modules.loadModule("text_editor", {}, null, "\u003cdiv data-pats=\"post_editor\" class=\"wdm_wrapper clearFix\" style=\"margin-top:5px;\"\u003e\n    \u003clabel style=\"display:inline-block;\"\u003e\n        \u003cinput data-pats=\"details_rich_text_editor\"\n               type=\"radio\"\n               name=\"main_def_editor_{{options.postfix}}\"\n               class=\"editor_rte\" style=\"margin-top: -5px;\"\n               {{#ifEquals options.editorType \"rte\"}}checked{{/ifEquals}} onclick=\"PEM.fire('toggleRichtext', ['rte', '{{options.postfix}}'])\"\u003e\n        Rich text editor\n    \u003c/label\u003e\u0026nbsp;\u0026nbsp;\u003clabel style=\"display:inline-block;\"\u003e\n        \u003cinput data-pats=\"details_plain_text_editor\"\n               type=\"radio\"\n               name=\"main_def_editor_{{options.postfix}}\"\n               class=\"editor_plain\" style=\"margin-top: -5px;\"\n               {{#ifEquals options.editorType \"plain\"}}checked{{/ifEquals}}  onclick=\"PEM.fire('toggleRichtext', ['plain', '{{options.postfix}}'])\"\u003e\n        Plain text editor\n      \u003c/label\u003e\u0026nbsp;\u0026nbsp;\n      \u003clabel style=\"display:inline-block;\"\u003e\n        \u003cinput data-pats=\"details_markdown_editor\"\n               type=\"radio\"\n               name=\"main_def_editor_{{options.postfix}}\"\n               class=\"editor_md\" style=\"margin-top: -5px;\"\n               {{#ifEquals options.editorType \"md\"}}checked{{/ifEquals}} onclick=\"PEM.fire('toggleRichtext', ['md', '{{options.postfix}}'])\"\u003e\n        Markdown editor\n    \u003c/label\u003e\n    \u003cspan class=\"input_links grey_text small_grey_text\" style=\"margin:4px 0 8px;float: right;\"\u003eReport any editor feedback to \u003ca href=\"mailto:bugs@piazza.com\"\u003ebugs@piazza.com\u003c/a\u003e\u003c/span\u003e\n    \u003cdiv id=\"incompatible_editor_warning\" style=\"color: #900;display: none; margin:2px\"\u003eThis post was last edited with rich text formatting. We’ve opened this post in the rich text editor to avoid any change in formatting.\u003c/div\u003e\n    \u003cdiv class=\"clearfix\"\u003e\u003c/div\u003e\n\n    \u003cdiv class=\"editor-row {{#unless livePreview}}no-preview{{/unless}}\"\u003e\n      \u003cdiv class=\"col first\"\u003e\n        \u003cdiv class=\"wmd-panel\" id=\"editor_{{options.postfix}}\"\u003e\n        \u003c!--\u003cdiv id=\"wmd-button-bar{{options.postfix}}\"\u003e\u003c/div\u003e--\u003e\n        \u003ctextarea data-pats=\"editor_textarea\" class=\"editor_textarea\" id=\"rich_{{options.postfix}}\" {{#if options.height}}style=\"height:{{options.height}}px;\"{{/if}}\n        onkeydown=\"P.text_editor.editorKeyDown(event, '{{options.postfix}}')\" onkeyup=\"P.text_editor.livePreview('{{options.postfix}}')\"\u003e\u003c/textarea\u003e\n      \u003c/div\u003e\n      \u003c/div\u003e\n      \u003cdiv class=\"col second post_region_content\" style=\"line-height: 17px;\"\u003e\n        \u003cdiv class=\"post_region_text preview_box\" id=\"preview_{{options.postfix}}_1\" style=\"display:none\"\u003e\u003c/div\u003e\n        \u003cdiv class=\"post_region_text preview_box\" id=\"preview_{{options.postfix}}_2\" style=\"display:none\"\u003e\u003c/div\u003e\n        \u003cdiv class=\"post_region_text preview_box\" style=\"height:auto;display:none;\" id=\"preview_height_{{options.postfix}}\"\u003e\u003c/div\u003e\n      \u003c/div\u003e\n      \u003cdiv class=\"clearfix\"\u003e\u003c/div\u003e\n    \u003c/div\u003e\n    \u003c!--\u003cdiv id=\"wmd-preview{{options.postfix}}\" class=\"wmd-panel wmd-preview\"\u003e\u003c/div\u003e--\u003e\n    \u003cspan class=\"input_links grey_text md_help\" style=\"font-size: 11px;color: #888;float:left;margin-top: 1px;\"\u003e\n\n   {{#ifEquals options.editorType \"plain\"}}\n   Use \u003cspan\u003e$\u003c/span\u003e$latex formula\u003cspan\u003e$\u003c/span\u003e$ for LaTeX.\n   {{/ifEquals}}\n   {{#ifEquals options.editorType \"md\"}}\n   We support \u003ca href=\"https://commonmark.org/help/\" target=\"_blank\"\u003eCommonMark\u003c/a\u003e Markdown specification.\n   Use \u003cspan\u003e$\u003c/span\u003e$latex formula\u003cspan\u003e$\u003c/span\u003e$ for LaTeX.\n   {{/ifEquals}}\n   \u003c/span\u003e\n    \u003cdiv class=\"clearfix\"\u003e\u003c/div\u003e\n    \u003cdiv class=\"actions\" style=\"margin-top:8px;\"\u003e\n        {{#if options.showIOnly}}\n        \u003cdiv style=\"font-size:12px;\"\u003eVisible to\n            \u0026nbsp;\u003clabel style=\"display:inline-block;font-size:12px;\"\u003e\n                \u003cinput type=\"radio\"\n                       name=\"ionly_{{options.postfix}}\"\n                       class=\"editor_plain\" style=\"margin-top: -2px;\"\n                       checked\u003e\n                everyone who sees this post\n            \u003c/label\u003e\u0026nbsp;\u0026nbsp;\u0026nbsp;\n            \u003clabel style=\"display:inline-block;font-size:12px;\"\u003e\n                \u003cinput type=\"radio\"\n                       name=\"ionly_{{options.postfix}}\"\n                       class=\"editor_md\" style=\"margin-top: -2px;\"\n                       id=\"ionly_{{options.postfix}}\"\n                       data-pats=\"instructors_only_button\"\u003e\n                Instructors only\n            \u003c/label\u003e\n        \u003c/div\u003e\n        {{/if}}\n        {{#unless options.noSave}}\n        \u003ca data-pats=\"post_link\" class=\"action save main submit_post\" href=\"#\" onclick=\"return P.text_editor.save('{{options.postfix}}')\"\u003e{{#if options.saveText}}{{options.saveText}}{{else}}save{{/if}}\u003c/a\u003e\n        {{#if options.anon}}\n        \u003cspan  class=\"action\"\u003e\n        \u0026nbsp;\u0026nbsp;as\n        \u003cselect id=\"anon_{{options.postfix}}\" class=\"anonymity_selector\"\u003e\n            {{#each options.anon}}\n            \u003coption value=\"{{this.val}}\"\u003e{{this.title}}\u003c/option\u003e\n            {{/each}}\n        \u003c/select\u003e\n        \u003c/span\u003e\n        {{/if}}\n        \u003ca id=\"autosave_link_{{options.postfix}}\" data-pats=\"save_draft_link\" class=\"action save_draft\" href=\"#\" onclick=\"return P.text_editor.autoSave('{{options.postfix}}', true)\" style=\"margin-left:10px;\"\u003esave draft\u003c/a\u003e\n        \u003ca class=\"action preview_link\" id=\"preview_link_{{options.postfix}}\" href=\"#\" onclick=\"return P.text_editor.handlePreviewButton('{{options.postfix}}', true);\" style=\"margin-left:10px;\"\u003epreview\u003c/a\u003e\n        \u003ca class=\"action right cancel\" href=\"#\" onclick=\"return P.text_editor.cancel('{{options.postfix}}')\" style=\"margin-left:10px;\"\u003ecancel\u003c/a\u003e\n        {{/unless}}\n    \u003c/div\u003e\n    \u003c!-- Adding this so it shows up in every text editor thing and it doesn't have to be place everywhere individually--\u003e\n    \u003cdiv class=\"new_feature_notification_wrapper\" {{#unless showTip}}style=\"display:none;\"{{/unless}}\u003e\n    \u003cdiv class=\"new_feature_notification\"\u003e\n        \u003cspan class=\"new_feature_close\" onclick=\"return P.text_editor.closeHelpTip()\"\u003e×\u003c/span\u003e\n        \u003ch3 class=\"new_feature_header\"\u003eNew Feature: Tag Instructors\u003c/h3\u003e\n        \u003cdiv class=\"new_feature_description_container\"\u003e\n            \u003cp class=\"new_feature_description\"\u003e\n            \u003cspan class=\"inst_only_tag_helper\" style=\"display: none;\"\u003e\n            Instructors can now tag fellow instructors in their posts by typing the @ symbol followed by their name, e.g. @Jane Smith.\n            \u003c/span\u003e\n                \u003cspan class=\"stud_only_tag_helper\"\u003e\n            Students and instructors can now tag instructors in their posts by typing the @ symbol followed by their name, e.g. @Jane Smith.\n            \u003c/span\u003e\n                When an instructor is tagged, they receive a real-time email bypassing their email notifications.\n            \u003c/p\u003e\n            \u003cp class=\"new_feature_description\"\u003e\n            \u003cspan class=\"inst_only_tag_helper\" style=\"display: none;\"\u003e\n            By default, only instructors can tag fellow instructors but you can enable students to also tag instructors if you wish.\n            You can change default settings from the Manage Class tab.\n            Instructors will see instructor names and emails in the auto complete.\n            If enabled for students, students will \u003cu\u003e\u003cb\u003eonly\u003c/b\u003e\u003c/u\u003e see instructor names and not instructor emails.\n            \u003c/span\u003e\n            \u003c/p\u003e\n            \u003cp class=\"new_feature_description\"\u003e\n                You can still link to posts by typing the @ symbol followed by the post number, e.g. @12\n            \u003c/p\u003e\n            \u003ca href=\"#\" class=\"new_feature_description\" style=\"margin-bottom: 10px; display: inline-block;\" onclick=\"return P.text_editor.closeHelpTip()\"\u003eOk got it\u003c/a\u003e\n            \u003cp class=\"new_feature_description\"\u003e\n                Feedback? Email us at \u003ca href=\"mailto:team@piazza.com\"\u003eteam@piazza.com\u003c/a\u003e\n            \u003c/p\u003e\n        \u003c/div\u003e\n    \u003c/div\u003e\n\u003c/div\u003e\n\u003c/div\u003e\n");</script>

<script type="text/javascript">
P.modules.loadModule("push", {}, null, "\u003c!-- no HTML needed --\u003e");</script>

<script type="text/javascript">
P.modules.loadModule("user_careers_profile", {}, null, "\u003cdiv id=\"careers_profile\" class=\"{{#if forStudent}}student_view{{/if}}{{#if forCompany}}company_view{{/if}}\" uid=\"{{profile.user_id}}\" style=\"display:block;\"\u003e\n  {{#if canAdmin}}\n  \u003cdiv class=\"profile_body profile_nav clearfix\"\u003e\n    {{#if fromSearch}}\n    \u003ca href=\"#/search\" class=\"show_for_company action_link back_to_search\"\u003eBack to Search\u003c/a\u003e\n    {{/if}}\n    \u003cdiv class=\"profile_container_full\"\u003e\n      \u003ca href=\"#\" class=\"profile_nav_btn profile_nav_btn_previous\"\u003e\u003cspan class=\"circle_wrap left\"\u003e\u003ci class=\"icon-chevron-left\"\u003e\u003c/i\u003e\u003c/span\u003ePrevious Student\u003c/a\u003e\n      \u003cdiv class=\"company_status\"\u003e\n        Status:\n        \u003cselect class=\"status_value\" uid=\"{{profile.user_id}}\"\u003e\n          \u003coption value=\"-\" checked\u003e-\u003c/option\u003e\n          \u003coption value=\"Contacted\"\u003eContacted\u003c/option\u003e\n          \u003coption value=\"Interview Scheduled\"\u003eInterview Scheduled\u003c/option\u003e\n          \u003coption value=\"Interviewing\"\u003eInterviewing\u003c/option\u003e\n          \u003coption value=\"Phone Screened\"\u003ePhone Screened\u003c/option\u003e\n          \u003coption value=\"Offer Made\"\u003eOffer Made\u003c/option\u003e\n          \u003coption value=\"Hired\"\u003eHired\u003c/option\u003e\n          \u003coption value=\"Rejected\"\u003eRejected\u003c/option\u003e\n          \u003coption value=\"Other\"\u003eOther\u003c/option\u003e\n        \u003c/select\u003e\n      \u003c/div\u003e\n      \u003ca href=\"#\" class=\"profile_nav_btn profile_nav_btn_next\"\u003eNext Student\u003cspan class=\"circle_wrap right\"\u003e\u003ci class=\"icon-chevron-right\"\u003e\u003c/i\u003e\u003c/span\u003e\u003c/a\u003e\n    \u003c/div\u003e\n  \u003c/div\u003e\n  {{/if}}\n  {{#if user.showCareers}}\n  {{#if showPrivacy}}\n  \u003cdiv class=\"post_region_box_profile message profile_privacy_message\"\u003e\n    \u003cbutton type=\"button\" class=\"close close_privacy_msg\" data-dismiss=\"alert\"\u003e\u0026times;\u003c/button\u003e\n    This profile is \u003cstrong\u003enot visible to students or instructors\u003c/strong\u003e in your class and only visible to companies after you publish it.\n  \u003c/div\u003e\n  {{/if}}\n  {{/if}}\n  {{#if profile.onCompany}}\n\n\u003c!--   \u003cdiv class=\"clearfix careers_tagging\"\u003e\n    \u003cdiv class=\"clearfix career_tags_header\"\u003e\n      \u003cul class=\"careers_tag_list clearfix profile_company_tag_list\" {{#unless profile.company_tags}}style=\"display:none;\"{{/unless}}\u003e\n        \u003cli\u003e\u003cspan class=\"profile_label\"\u003eTags for this student:\u003c/span\u003e\u003c/li\u003e\n        \u003cdiv class=\"selected_tags\"\u003e\n        {{#each profile.company_tags}}\n        \u003cli\u003e\u003ca href=\"#\" class=\"tag folder selected\" onclick=\"return false;\"\u003e{{this}}\u003c/a\u003e\u003c/li\u003e\n        {{/each}}\n        \u003c/div\u003e\n      \u003c/ul\u003e\n      \u003ca href=\"#\" class=\"action_link show_tagging\" style=\"float:right;\"\u003eTag this student\u003c/a\u003e\n      \u003ca href=\"#\" class=\"action_link hide_tagging\" style=\"float:right;display:none\"\u003eDone tagging\u003c/a\u003e\n    \u003c/div\u003e\n\n    \u003cdiv class=\"post_region_box_profile clearfix tagging_wrapper\" style=\"display:none;\"\u003e\n      \u003ch4\u003eTag This Student\u003c/h4\u003e\n      \u003cul class=\"careers_tag_list clearfix tag_list_profile\"\u003e\n        {{#each allTags}}\n        \u003cli tag=\"{{this}}\" class=\"li_tag_wrapper\"\u003e\u003ca href=\"#\" class=\"tag folder {{#ifContains ../profile.company_tags this}}selected{{/ifContains}}\" onclick=\"return P.user_careers_profile.toggleTag(this);\"\u003e{{this}}\u003c/a\u003e\u003c/li\u003e\n        {{/each}}\n      \u003c/ul\u003e\n      \u003cdiv class=\"clearfix create_tag_section\"\u003e\n        \u003cinput type=\"text\" class=\"new_tag_textbox\" placeholder=\"Enter tag name\"\u003e\n        \u003cbutton class=\"btn btn-small create_new_tag\"\u003eCreate New Tag\u003c/button\u003e\n      \u003c/div\u003e\n      \u003ca href=\"#manage_careers_tags\" role=\"button\" data-toggle=\"modal\" class=\"action_link\"\u003eManage Tags\u003c/a\u003e\n    \u003c/div\u003e\n  \u003c/div\u003e --\u003e\n\n  \u003cdiv class=\"clearfix careers_tagging\"\u003e\n    \u003cul class=\"careers_tag_list tag_library clearfix\"\u003e\n      \u003clabel\u003eTag Library:\u003c/label\u003e\n      {{#each allTags}}\n      \u003cli\u003e\u003ca href=\"#\" class=\"careers_tag {{#ifContains ../profile.company_tags this}}active{{/ifContains}}\" onclick=\"return P.user_careers_profile.toggleTagFromLibrary(this);\"\u003e{{this}}\u003cspan class=\"remove_tag_wrap\"\u003e\u003ci class=\"icon-remove-sign remove_tag\" onclick=\"return P.user_careers_profile.deleteTag(this);\"\u003e\u003c/i\u003e\u003c/span\u003e\u003c/a\u003e\u003c/li\u003e\n      {{/each}}\n      \u003ca href=\"#\" class=\"manage_tag_toggle start_manage_tag_toggle\"\u003eManage Tag Library\u003c/a\u003e\n      \u003ca href=\"#\" class=\"manage_tag_toggle done_manage_tag_toggle\"\u003eDone Managing\u003c/a\u003e\n    \u003c/ul\u003e\n    \u003cul id=\"selected_user_tags\" class=\"careers_tag_list clearfix\"\u003e\n      {{#each profile.company_tags}}\n      \u003cli\u003e\u003ca href=\"#\" class=\"careers_tag active\" onclick=\"return P.user_careers_profile.toggleTag(this);\"\u003e{{this}}\u003c/a\u003e\u003c/li\u003e\n      {{/each}}\n      \u003cli id=\"add_tags_input\"\u003e\n        \u003cinput type=\"text\" id=\"add_tags\" placeholder=\"Click to Add Tags\"\u003e\n        \u003cul class=\"tags_autocomplete\" style=\"display:none;\"\u003e\n        \u003c/ul\u003e\n      \u003c/li\u003e\n    \u003c/ul\u003e\n\n    \u003ca href=\"#\" class=\"btn btn-mini show_tag_toggle tag_toggle\"\u003e\u003ci class=\"icon-chevron-up\"\u003e\u003c/i\u003e Show Tag Library\u003c/a\u003e\n    \u003ca href=\"#\" class=\"btn btn-mini hide_tag_toggle tag_toggle\"\u003e\u003ci class=\"icon-chevron-down\"\u003e\u003c/i\u003e Hide Tag Library\u003c/a\u003e\n  \u003c/div\u003e\n  {{/if}}\n  {{#if user.showCareers}}\n  {{#if profile.me}}\n  {{#unless profile.profile_settings.published}}\n  \u003cdiv class=\"post_region_box_profile message hide_for_student_preview show_for_student clearfix\"\u003e\n    \u003cdiv class=\"message_wrapper\"\u003e\n      \u003ch4 class=\"message_header\"\u003e\n        Are you ready to publish your profile?\n      \u003c/h4\u003e\n      \u003cp\u003ePublish your profile to allow companies to learn more about you.\u003c/p\u003e\n    \u003c/div\u003e\n    \u003cbutton class=\"btn btn-mini publish_button btn-primary publish_profile\"\u003ePublish Profile\u003c/button\u003e\n  \u003c/div\u003e\n  {{/unless}}\n  {{/if}}\n  {{/if}}\n  {{#if forStudent}}\n  \u003cdiv class='show_for_student_preview action_link stop_view_as_company'\u003e\u003ca href=\"#\" class=\"show_for_student action_link show_for_student_preview stop_view_as_company\"\u003eBack to student view of my profile\u003c/a\u003e\u003c/div\u003e\n  {{/if}}\n  {{#if user.showCareers}}\n  {{#if profile.me}}\n    \u003ca href=\"#\" class=\"action_link view_as_company\"\u003eSee company view of my profile\u003c/a\u003e\n  {{/if}}\n  {{/if}}\n  \u003cdiv class=\"post_region_box_profile header clearfix\"\u003e\n    \u003cdiv class=\"profile_header clearfix\"\u003e\n      {{#if profile.company_log}}\n         \u003cspan class=\"company_action_count\"\u003e\u003ca href=\"#company_actions_anchor\"\u003e{{profile.company_log.length}} Company Actions\u003c/a\u003e\u003c/span\u003e\n      {{/if}}\n      {{#if profile.me}}\n        \u003ca href=\"#\" class=\"show_for_student action_link edit_button hide_for_student_preview hide_for_edit edit_profile_header\"\u003eedit\u003c/a\u003e\n        \u003ca href=\"#\" class=\"show_for_student action_link save_button edit_profile_header show_for_edit\"\u003esave\u003c/a\u003e\n      {{/if}}\n      \u003cdiv class=\"profile_pic\"\u003e\n        {{#if profile.imagePath}}\n          \u003cimg id=\"thumb\" title=\"profile picture\" src=\"{{profile.imagePath}}\" onload=\"onImageLoad(event);\" style=\"width:100%; height: 100%;\"\u003e\n        {{else}}\n          \u003cimg id=\"thumb\" title=\"profile picture\" src=\"https://dvngeac8rg9mb.cloudfront.net/images/dashboard/common/default_user.png\" style=\"width:100%; height: 100%;\"\u003e\n        {{/if}}\n      \u003c/div\u003e\n      \u003cdiv class=\"profile_who\"\u003e\n        \u003ch2 class=\"profile_name\"\u003e{{profile.name}}\n        {{#if canAdmin}}\n        {{#if profile.requested_interview}}\n        \u003cspan class=\"label label-default\" style=\"background-color:#999999\";\u003eStudent Requested Interview\u003c/span\u003e\n        {{/if}}\n        {{/if}}\n        \u003c/h2\u003e\n        {{#if canAdmin}}\n        \u003cdiv class=\"fast_actions\"\u003e\n          \u003cdiv class=\"fast_actions_tips\" style=\"display:none;\"\u003e\n            \u003cdiv class=\"container\"\u003e\n              \u003ca href=\"#\" class=\"remove_btn remove_fast_actions_tips\"\u003eGot it \u003cspan class=\"icon-remove-circle\"\u003e\u003c/span\u003e\u003c/a\u003e\n              \u003ch3\u003e\n                We’ve added fast actions!\n                \u003cspan\u003eClick:\u003c/span\u003e\n              \u003c/h3\u003e\n            \u003c/div\u003e\n          \u003c/div\u003e\n          \u003ci class=\"favorite_icon {{#if profile.my_favorite}}active{{/if}} favorite_link\" tipsy=\"Favorite\"\u003e\u003c/i\u003e\n          {{#ifContains profile.archived myid}}\n            \u003ci class=\"archive_icon archive active\" tipsy=\"Un-archive\"\u003e\u003c/i\u003e\n          {{else}}\n            \u003ci class=\"archive_icon archive\" tipsy=\"Archive\"\u003e\u003c/i\u003e\n          {{/ifContains}}\n          \u003ci class=\"send_message_icon send_message\" tipsy=\"Send a message\"\u003e\u003c/i\u003e\n          {{#if profile.has_correspondence}}\n          \u003ci class=\"see_message_icon see_messages active show_for_messages\" tipsy=\"View past messages\"\u003e\u003cspan class=\"see_message_count\"\u003e{{profile.has_correspondence}}\u003c/span\u003e\u003c/i\u003e\n          {{else}}\n          \u003ci class=\"see_message_icon\"\u003e\u003cspan class=\"see_message_count\"\u003e0\u003c/span\u003e\u003c/i\u003e\n          {{/if}}\n          {{#if profile.resume}}\n          \u003ca href=\"{{profile.resume}}\" uid=\"{{profile.user_id}}\" class=\"action_link download_resume\" target=\"_blank\"\u003e\u003ci class=\"resume_icon active\" tipsy=\"Download resume\"\u003e\u003c/i\u003e\u003c/a\u003e\n          {{else}}\n          \u003ci class=\"resume_icon\"\u003e\u003c/i\u003e\n          {{/if}}\n        \u003c/div\u003e\n        {{/if}}\n        \u003ch3 class=\"profile_school\"\u003e\u003cspan class=\"hide_for_edit\"\u003e{{profile.school}}\u003c/span\u003e\n          \u003cspan class=\"show_for_edit\"\u003e\n          \u003cselect id=\"new_school_id\" data-field=\"school_id\"\u003e\n            {{#each profile.schools}}\n            \u003coption value=\"{{this.id}}\" {{#ifEquals this.name ../profile.school}}selected{{/ifEquals}}\u003e{{this.name}}\u003c/option\u003e\n            {{/each}}\n          \u003c/select\u003e\n          \u003c/span\u003e\n        \u003c/h3\u003e\n\n        {{#if canAdmin}}\n        {{#if profile.visa}}\n        \u003cp class=\"visa_sponsorship hide_for_not_published\"\u003eVisa sponsorship\n          \u003cspan class=\"sponsorship_info hide_for_edit\"\u003e{{profile.visa}}\u003c/span\u003e\n        \u003c/p\u003e\n        {{/if}}\n        {{#ifEquals profile.status \"accepted_fulltime\"}}\n          \u003cspan class=\"label\" style=\"background-color:#f89406\";\u003eAccepted Fulltime Offer\u003c/span\u003e\n        {{/ifEquals}}\n        {{#ifEquals profile.status \"accepted_intern\"}}\n          \u003cspan class=\"label\" style=\"background-color:#f89406\";\u003eAccepted Internship Offer\u003c/span\u003e\n        {{/ifEquals}}\n        {{else}}\n        \u003cp class=\"visa_sponsorship hide_for_not_published\"\u003eVisa sponsorship\n          \u003cspan class=\"sponsorship_info hide_for_edit\"\u003e{{#if profile.visa}}{{profile.visa}}{{else}}\u003ci\u003eunknown\u003c/i\u003e{{/if}}\u003c/span\u003e\n          \u003cspan class=\"show_for_edit add_margin\"\u003e\n            \u003clabel class=\"radio inline\"\u003e\n              \u003cinput name=\"sponsorship_needed\" type=\"radio\" value=\"required\" data-field=\"visa\" {{#ifEquals profile.visa \"required\"}}checked{{/ifEquals}}\u003e required\n            \u003c/label\u003e\n            \u003clabel class=\"radio inline\"\u003e\n              \u003cinput name=\"sponsorship_needed\" type=\"radio\" value=\"not required\" data-field=\"visa\" {{#ifEquals profile.visa \"not required\"}}checked{{/ifEquals}}\u003e not required\n            \u003c/label\u003e\n          \u003c/span\u003e\n        \u003c/p\u003e\n        {{/if}}\n        \u003c!-- \u003cp class=\"marketing show_for_company\"\u003eViewed your company profile\u003c/p\u003e --\u003e\n\n\n      \u003c/div\u003e\n\n      {{#if profile.me}}\n      \u003cdiv class=\"profile_contact hide_for_not_published clearfix\"\u003e\n        \u003cdiv class=\"entry email clearfix\"\u003e\n          \u003clabel class=\"email_icon\"\u003e\u003c/label\u003e\n          \u003cp class=\"entry entry_value email hide_for_edit\"\u003e{{{profile.allEmails}}}\u003c/p\u003e\n          \u003cinput type=\"text\" class=\"profile_text_input show_for_edit\" value=\"{{profile.allEmails2}}\" data-field=\"contact.custom_email\"\u003e\n        \u003c/div\u003e\n        \u003cdiv class=\"entry phone clearfix\"\u003e\n          \u003clabel class=\"phone_icon\"\u003e\u003c/label\u003e\n          \u003cp class=\"entry entry_value phone hide_for_edit\"\u003e{{profile.contact.phone}}\u003c/p\u003e\n          \u003cinput type=\"text\" class=\"profile_text_input show_for_edit\" value=\"{{profile.contact.phone}}\" data-field=\"contact.phone\"\u003e\n        \u003c/div\u003e\n        \u003cdiv class=\"entry skype clearfix\"\u003e\n          \u003clabel class=\"skype_icon\"\u003e\u003c/label\u003e\n          \u003cp class=\"entry entry_value hide_for_edit skype\"\u003e{{profile.contact.skype}}\u003c/p\u003e\n          \u003cinput type=\"text\" class=\"profile_text_input show_for_edit\" value=\"{{profile.contact.skype}}\" data-field=\"contact.skype\"\u003e\n        \u003c/div\u003e\n      \u003c/div\u003e\n      {{/if}}\n    \u003c/div\u003e\n    \u003cdiv class=\"profile_actions clearfix\"\u003e\n      \u003cdiv class=\"profile_actions_wrapper show_for_company left_align\"\u003e\n        \u003c!-- \u003ca href=\"#\" class=\"action_link archive\"\u003eArchive\u003c/a\u003e\n        \u003cspan class=\"middot\"\u003e\u0026middot;\u003c/span\u003e --\u003e\n        {{#if canAdmin}}\n\u003c!--         \u003ca href=\"#\" class=\"action_link favorite\"\u003e{{#if profile.my_favorite}}Unfavorite{{/if}}{{#unless profile.my_favorite}}Favorite{{/unless}}\u003c/a\u003e\n        \u003cspan class=\"middot\"\u003e\u0026middot;\u003c/span\u003e\n        \u003ca href=\"#\" class=\"action_link send_message\"\u003eSend Message\u003c/a\u003e\n        \u003cspan class=\"middot show_for_messages\" {{#unless profile.has_correspondence}}style=\"display:none;\"{{/unless}}\u003e\u0026middot;\u003c/span\u003e\n        \u003ca href=\"#\" class=\"action_link see_messages show_for_messages\" {{#unless profile.has_correspondence}}style=\"display:none;\"{{/unless}}\u003e\n          {{#showCompanyUsers profile.message_arr}}See Messages{{/showCompanyUsers}}\n        \u003c/a\u003e --\u003e\n        \u003cdiv class=\"dropdown profile_actions_dropdown\"\u003e\n          \u003ca href=\"#\" class=\"dropdown-toggle\" dat