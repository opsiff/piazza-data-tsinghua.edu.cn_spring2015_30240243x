  <script type="text/javascript">
PTM.createModuleTemplate("careers_dashboard", "careers_dashboard_event_story", "\u003cli class=\"company_story clearfix\" id=\"{{item.id}}\" school=\"{{this.school}}\" company=\"{{item.config.company_info.internal_id}}\"\u003e\n  \u003cdiv class=\"company_logo homescreen_company_logo\"\u003e\n    \u003ca href=\"#\" class=\"company_link\" company=\"{{item.config.company_info.internal_id}}\"\u003e\u003cimg src=\"{{item.config.company_info.image}}\"\u003e\u003c/a\u003e\n  \u003c/div\u003e\n  \u003cdiv class=\"company_content_wrapper\"\u003e\n    \u003ch2\u003e{{{item.body}}}\u003c/h2\u003e\n    \u003cdiv class=\"company_content clearfix\"\u003e\n      \u003cdiv class=\"clearfix\"\u003e\n        \u003cdiv class=\"company_event_blurb\"\u003e\n          \u003cp\u003e{{{item.config.event_info.summary}}}\u003c/p\u003e\n        \u003c/div\u003e\n        \u003cul class=\"event_info clearfix\"\u003e\n          {{#if item.config.event_info.date}}\n          \u003cli class=\"clearfix\"\u003e\n            \u003ci class=\"icon-calendar\"\u003e\u003c/i\u003e\n            \u003cspan\u003e{{item.config.event_info.date}}\u003c/span\u003e\n          \u003c/li\u003e\n          {{/if}}\n          {{#if item.config.event_info.time}}\n          \u003cli class=\"clearfix\"\u003e\n            \u003ci class=\"icon-time\"\u003e\u003c/i\u003e\n            \u003cspan\u003e{{item.config.event_info.time}}\u003c/span\u003e\n          \u003c/li\u003e\n          {{/if}}\n          {{#if item.config.event_info.where}}\n          \u003cli class=\"clearfix\"\u003e\n            \u003ci class=\"icon-map-marker\"\u003e\u003c/i\u003e\n            \u003cspan\u003e{{item.config.event_info.where}}\u003c/span\u003e\n          \u003c/li\u003e\n          {{/if}}\n        \u003c/ul\u003e\n      \u003c/div\u003e\n      \u003cdiv class=\"follow_actions_wrapper clearfix\"\u003e\n        {{#if item.config.following}}\n        \u003cdiv class=\"stop_follow_wrapper\"\u003e\n          \u003cdiv class=\"clearfix\"\u003e\n            \u003cbutton class=\"btn btn-primary btn-small do-unfollow-event\" event=\"{{item.id}}\" company=\"{{item.config.company_info.internal_id}}\"\u003eStop Following This Event\u003c/button\u003e\n            \u003cspan\u003eYou are currently \u003cbr\u003efollowing this event.\u003c/span\u003e\n          \u003c/div\u003e\n          \u003cdiv class=\"event-like-cd\"\u003e\u003c/div\u003e\n        \u003c/div\u003e\n        {{else}}\n        \u003cdiv class=\"follow_wrapper\"\u003e\n          \u003cdiv class=\"clearfix\"\u003e\n            \u003cbutton class=\"btn btn-primary btn-small do-follow-event\" event=\"{{item.id}}\" cname=\"{{item.config.company_info.name}}\" company=\"{{item.config.company_info.internal_id}}\"\u003eFollow This Event\u003c/button\u003e\n            \u003cspan\u003eFollowing this event keeps you \u003cbr\u003eposted with event updates.\u003c/span\u003e\n          \u003c/div\u003e\n          \u003cdiv class=\"event-like-cd\"\u003e\u003c/div\u003e\n        \u003c/div\u003e\n        {{/if}}\n      \u003c/div\u003e\n      {{#if item.config.followers}}\n      \u003cdiv class=\"follower_number clearfix\"\u003e\n        \u003ci class=\"icon-user\"\u003e\u003c/i\u003e\n        \u003cspan\u003e{{item.config.followers}} student(s) following this event\u003c/span\u003e\n      \u003c/div\u003e\n      {{/if}}\n    \u003c/div\u003e\n  \u003c/div\u003e\n\u003c/li\u003e");</script>

