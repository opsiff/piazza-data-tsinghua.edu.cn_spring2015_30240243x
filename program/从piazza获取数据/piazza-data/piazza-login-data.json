PTM.createTemplate("student_classmate_popover", "{{#if canShow}}\n\u003cdiv class=\"student_popover student_popover_jk34jk2k4jk3\"\u003e  \n  \u003cdiv class=\"popover_arrow\"\u003e\u003c/div\u003e\n  \u003cdiv class=\"popover_top_section\"\u003e\n    \u003cdiv class=\"popover_row clearfix\"\u003e\n      \u003cdiv class=\"left_column clearfix pull-left\"\u003e\n        \u003cimg class=\"student_popover_img pull-right\" src=\"{{imgUrl}}\" alt=\"prof\"\u003e\n      \u003c/div\u003e\n      \u003cdiv class=\"right_column pull-left\"\u003e\n        \u003ca href=\"/classmates/{{nid}}/{{id}}\" target=\"_blank\" class=\"student_popover_name\"\u003e{{name}}\u003c/a\u003e\n        \u003cdiv class=\"student_popover_meta\"\u003e{{studentStatus}} {{gradYear}} {{gradMonth}}\u003c/div\u003e\n        {{#if major}}\n        \u003cdiv class=\"student_popover_major\"\u003eMajor: {{major}}\u003c/div\u003e\n        {{/if}}\n        \u003cdiv class=\"popover_divider_line\"\u003e\u003c/div\u003e\n      \u003c/div\u003e\n    \u003c/div\u003e\n  \u003c/div\u003e\n  \u003cdiv class=\"popover_mid_section\"\u003e\n    \u003cdiv class=\"popover_row classes_row clearfix\"\u003e\n      \u003cdiv class=\"left_column pull-left\"\u003eClasses\u003c/div\u003e\n      \u003cdiv class=\"right_column pull-left\"\u003e\n        {{#forLoop classes elem=\"span\"}}\n        \u003cdiv class=\"class_container clearfix\"\u003e\n          {{#if isTA}}\n          \u003cspan class=\"class_badge ta_badge\"\u003eTA\u003c/span\u003e\n          {{/if}}\n          {{#if isTS}}\n          \u003cspan class=\"class_badge ts_badge\"\u003eTS\u003c/span\u003e\n          {{/if}}\n          \u003cspan class=\"class_name\"\u003e{{number}} {{name}}\u003c/span\u003e\n        \u003c/div\u003e\n        {{/forLoop}}\n        {{#if classesMore}}\n        \u003ca href=\"/classmates/{{nid}}/{{id}}\" target=\"_blank\"\u003e{{classesMore}} more\u003c/a\u003e\n        {{/if}}\n      \u003c/div\u003e                  \n    \u003c/div\u003e\n    {{#if workedAt}}\n    \u003cdiv class=\"popover_row worked_at_row clearfix\"\u003e\n      \u003cdiv class=\"left_column pull-left\"\u003eWorked\u003cbr\u003eat\u003c/div\u003e\n      \u003cdiv class=\"right_column pull-left\"\u003e\n        {{#forLoop workedAt}}\n          \u003cdiv class=\"company_container clearfix\"\u003e\n            \u003cspan class=\"company_name\"\u003e{{company}}\u003c/span\u003e\n            {{#if role}}\n            \u003cspan class=\"company_job_title\"\u003e, {{role}}\u003c/span\u003e\n            {{/if}}\n            {{#if city}}\n            \u003cspan class=\"company_location\"\u003e, {{city}}\u003c/span\u003e\n            {{/if}}\n          \u003c/div\u003e\n        {{/forLoop}}\n        {{#if workedMore}}\n        \u003ca href=\"/classmates/{{nid}}/{{id}}\" target=\"_blank\"\u003e{{workedMore}} more\u003c/a\u003e\n        {{/if}}\n      \u003c/div\u003e\n    \u003c/div\u003e\n    {{/if}}\n  \u003c/div\u003e\n\n  \u003cdiv class=\"popover_footer_section\"\u003e\n    \u003ca href=\"/classmates/{{nid}}?from=card\" target=\"_blank\"\u003eSee All Classmates\u003c/a\u003e\n  \u003c/div\u003e\n\n\n\u003c/div\u003e\n{{else}}\n\u003cdiv class=\"student_popover student_popover_jk34jk2k4jk3\"\u003e  \n  \u003cdiv class=\"popover_arrow\"\u003e\u003c/div\u003e\n  \u003cdiv class=\"popover_mid_section clearfix unpublished\"\u003e\n    \n    \u003cdiv\u003e\n      To see \u003cspan class=\"bold\"\u003e{{name}}\u003c/span\u003e's Piazza profile you need to enable community features and publish your profile\n    \u003c/div\u003e\n    \u003ca href=\"#\" onclick=\"P.top_bar.isRefresh=true;return P.homescreen.optInCaag(true, 'student_popover');\"\u003eEnable\u003c/a\u003e\n    \n  \u003c/div\u003e\n\u003c/div\u003e\n{{/if}}\n");</script>

  <script type="text/javascript">
PTM.createTemplate("question_note_view", "      \u003cdiv id=\"poll_admin\" class=\"dashboard_element remove_on_post_load\"\u003e\u003c/div\u003e\n      \u003ca id=\"must_read_button\" class=\"btn btn-primary btn-primary-red btn-mini remove_on_post_load\" style=\"margin-left:10px;margin-top:10px;font-weight:bold;display:none\" href=\"#\"\u003eI’ve read this \u003ci\u003e(removes from reading list bin)\u003c/i\u003e\u003c/a\u003e\n      \u003ca id=\"undo_must_read_button\" class=\"btn btn-primary btn-mini remove_on_post_load\" style=\"margin-left:10px;margin-top:10px;font-weight:bold;display:none\" href=\"#\"\u003eAdd back to reading list bin\u003c/a\u003e\n      \u003cdiv data-pats=\"original_post\" id=\"{{content.type}}\" class=\"post_region_box question_note_view dashboard_element\"\u003e\n        {{#if content.is_dupe}}\n        \u003cdiv class=\"duplicate\" id=\"question_is_dupe\"\u003e\n          \u003cspan class=\"message\"\u003eThe post you're trying to view was marked a duplicate of this post, and is appended as a followup below.\u003c/span\u003es\n        \u003c/div\u003e\n        {{/if}}\n        \u003cdiv class=\"duplicate\" id=\"mark_dupe_form\"\u003e\n          \u003col\u003e\n            \u003cli\u003eScroll the left panel to find which question this is a duplicate of and click on it\u003c/li\u003e\n            \u003cli\u003eClick \"Mark as Duplicate\" button to complete\u003c/li\u003e\n          \u003c/ol\u003e\n          \u003cul\u003e\n            \u003cli\u003eDuplicate question: \u003cspan id=\"duplicate_title\"\u003e\u003c/span\u003e\u003c/li\u003e\n            \u003cli\u003eOptional explanation: \u003cinput type=\"text\" id=\"duplicate_reason\" /\u003e\u003c/li\u003e\n            \u003cli\u003eMarking duplicate of: \u003cspan id=\"duplicate_of_title\"\u003e\u003cb\u003e\u003ci\u003e-please select from left panel-\u003c/i\u003e\u003c/b\u003e\u003c/span\u003e\u003c/li\u003e\n          \u003c/ul\u003e\n          \u003cbutton disabled=\"disabled\" class=\"btn btn-danger btn-small\" id=\"mark_duplicate\" onclick=\"PEM.fire('question_mark_duplicate_submit')\"\u003eMark as Duplicate\u003c/button\u003e\n          \u003cbutton class=\"btn btn-small\" onclick=\"PEM.fire('question_mark_duplicate_cancel')\"\u003eCancel\u003c/button\u003e\n        \u003c/div\u003e\n      \u003cdiv class=\"post_region_header clearFix\"\u003e\n        \u003cdiv class=\"post_icon {{content.type}} {{#if content.no_answer}}unanswered{{/if}}\"\u003e\u003c/div\u003e\n        {{#if content.isTeammateSearch}}\n        \u003cdiv class=\"post_title\"\u003eTeammate Search\u003cem style=\"color: #468AB8;\"\u003e (Posted by Piazza)\u003c/em\u003e\u003c/div\u003e\n        {{else}}\n        \u003cdiv data-pats=\"post_type_text\" class=\"post_title\"\u003e{{#if content.isGroup}}group {{else}}{{#ifEquals content.status \"private\"}}private {{/ifEquals}}{{/if}}{{content.type}}\u003c/div\u003e\n        {{/if}}\n        \u003cdiv class=\"post_favorite copylink\"\u003e\n          \u003cdiv class=\"pull-left clearfix\" style=\"margin-top: -4px;margin-right: 5px; cursor: default;color: #555;cursor: pointer;\" id=\"at_ref_top\" onclick=\"P.note_view.copyAt();return false;\"\u003e@{{content.nr}}\u003c/div\u003e\n          \u003ci class=\"question_icon link pull-left\" onclick=\"P.note_view.copyLink();return false;\"\u003e\u003c/i\u003e\n          \u003cdiv class=\"pull-left link_copied\" id=\"link_copied_top_level\" style=\"display:none;\"\u003eURL copied!\u003c/div\u003e\n        \u003c/div\u003e\n        {{#unless content.config.live}}\n        \u003cdiv class=\"post_favorite favorite {{#if content.my_favorite}}is_favorite{{else}}is_not_favorite{{/if}}\"\u003e\n          \u003ci class=\"icon_favorite is_favorite\" onclick=\"PEM.fire('favorite', false);return false;\"\u003e\u003c/i\u003e\n          \u003ci class=\"icon_favorite not_favorite\" onclick=\"PEM.fire('favorite', true);return false;\" tutorial=\"Save this post to your favorites to find it later.\"\u003e\u003c/i\u003e\n        \u003c/div\u003e\n        {{/unless}}\n        {{#if user.isInst}}\n        \u003cdiv class=\"dropdown premium-disabled-tipsy premium3\" style=\"display: inline-block\"\u003e\n        \u003cbutton class=\"transparent premium-disabled-noclick\" style=\"margin-left: 5px\" onclick=\"$('#content_lock_menu').show();event.stopPropagation();\"\u003e\n          \u003cimg src=\"/images/unlock_icon.png\" class=\"content-active\" alt=\"lock_icon\" width=\"12\" height=\"14\" style=\"\n    margin: -4px 0 0 0;\n\"/\u003e\n          \u003cimg src=\"/images/lock_icon.png\" class=\"content-inactive\" alt=\"lock_icon\" width=\"12\" height=\"14\" style=\"\n    margin: -4px 0 0 0; display:none;\" /\u003e\n          \u003cb class=\"\"\u003e\u0026blacktriangledown;\u003c/b\u003e\n        \u003c/button\u003e\n        \u003cdiv class=\"dropdown-menu general_dropdown\" id=\"content_lock_menu\" style=\"padding: 10px 10px; width: 280px; font-weight: normal;\" onclick=\"event.stopPropagation();\"\u003e \u003c!-- @renars - toggling .show opens and closes this --\u003e\n          \u003cul onclick=\"event.stopPropagation();\"\u003e\n          \u003cli class=\"content-inactive\"\u003e\n            \u003ca onclick=\"P.note_view.lockPost(false)\"\u003eUnlock this post now\u003c/a\u003e\n          \u003c/li\u003e\n          \u003cli class=\"content-active\"\u003e\u003clabel\u003e\n            \u003cinput type=\"radio\" name=\"lockRadio\" id=\"lock_post_now\"\u003e\n            \u003cspan style=\"vertical-align: sub\"\u003e Lock this post immediately\u003c/span\u003e\n          \u003c/label\u003e\u003c/li\u003e\n          \u003cli class=\"content-active\"\u003e\u003clabel\u003e\n            \u003cinput type=\"radio\" name=\"lockRadio\" id=\"lock_post_later\"\u003e\n              \u003cspan style=\"vertical-align: sub\"\u003e Schedule future lock date/time\u003c/span\u003e\n            \u003c/label\u003e\n\n            \u003cinput type=\"text\" id=\"inactive_date\" style=\"width: 100px;\" value=\"{{ content.config.lock.date }}\"\u003e\n            \u003cinput type=\"text\" id=\"inactive_time\" style=\"width: 70px;\" value=\"{{ content.config.lock.time }}\"\u003e {{ timezone }}\n          \u003c/li\u003e\n          \u003c/ul\u003e\n          \u003cdiv class=\"content-active\"\u003e\n            \u003cbutton class=\"btn-primary\" onclick=\"P.note_view.lockPost(true)\"\u003eSave changes\u003c/button\u003e\n            \u003ca href=\"\" style=\"display: inline-block; margin-left: 5px\" onclick=\"closeDropDown();return false;\"\u003ecancel\u003c/a\u003e\n          \u003c/div\u003e\n        \u003c/div\u003e\n      \u003c/div\u003e\n        {{/if}}\n        {{#if content.config.live}}\n        \u003cdiv class=\"like_button_wrapper live_content\" id=\"upvote_button_wrapper\" style=\"margin-top:0px;\"\u003e\n          \u003cbutton id=\"upvote_button\" class=\"btn btn-small\" style=\"line-height:15px; margin-top:2px;\" onclick=\"P.question_view.upvote()\"\u003eUpvote\u003c/button\u003e\n          \u003cspan id=\"upvote_button_done\" style=\"display:inline-block;margin-top:5px;\"\u003eUpvoted!\u003c/span\u003e\n        \u003c/div\u003e\n        \u003cdiv class=\"like_button_wrapper live_content\" id=\"upvote_close_button_wrapper\" style=\"margin-top:0px;\"\u003e\n          \u003cbutton id=\"upvote_close_button\" class=\"btn btn-small\" style=\"line-height:15px; margin-top:2px;\" onclick=\"P.question_view.upvoteClose()\"\u003eClose upvotes\u003c/button\u003e\n          \u003cspan id=\"upvote_close_button_done\" style=\"display:inline-block;margin-top:5px;\"\u003eUpvotes closed\u003c/span\u003e\n        \u003c/div\u003e\n        {{/if}}\n        {{#if network.isOpen}}\n        \u003cdiv class=\"like_button_wrapper\" id=\"like_button_wrapper\"\u003e\n          \u003cspan class=\"like_button_icon\"\u003e\u003c/span\u003e\n          \u003cspan\u003e\u003ca href=\"#\" onclick=\"return PEM.fire('like', true);\"\u003eLike\u003c/a\u003e\u003c/span\u003e\n        \u003c/div\u003e\n        {{/if}}\n        \u003cdiv class=\"post_converted_message\"\u003e{{#if content.isQuestion}}Your note is now a question.{{else}}Your question is now a note.{{/if}}\u003cdiv class=\"post_converted_message_close\"\u003ex\u003c/div\u003e\u003c/div\u003e\n        \u003cdiv class=\"post_view_count\"\u003e\u003cspan class=\"count\"\u003e{{content.unique_views}}\u003c/span\u003e views\u003c/div\u003e\n        \u003ca href=\"#\" class=\"follow_link {{#unless content.is_bookmarked}}hide{{/unless}}\" onclick=\"PEM.fire('question_note_stop_follow');return false;\"\u003estop following\u003c/a\u003e\n        {{#if content.config.live}}\n        {{#if user.isInst}}\n        \u003cdiv class=\"live_content\" style=\"margin-top:0px;float:right;margin-right:8px;\"\u003e\n          \u003cbutton class=\"btn btn-small\" style=\"line-height:15px; margin-top:2px;\" onclick=\"P.question_view.addToClass()\"\u003eMove to class feed\u003c/button\u003e\n        \u003c/div\u003e\n        {{/if}}\n        {{/if}}\n      \u003c/div\u003e\n      {{#unless content.isTeammateSearch}}\n      {{#unless content.config.live}}\n      \u003cdiv class=\"dropdown post_region_arrow_dropdown qa_actions_dropdown\"\u003e\n        \u003ca href=\"#\" class=\"dropdown-toggle\"\u003eActions \u003cb class=\"caret\"\u003e\u003c/b\u003e\u003c/a\u003e\n        \u003cdiv class=\"dropdown_menu_wrapper clearFix\"\u003e\n          \u003cul class=\"dropdown-menu\" role=\"menu\" id=\"question_note_actions_dropdown\" aria-labelledby=\"dLabel\"\u003e\n            {{#if content.is_bookmarked}}\n              \u003cli onclick=\"PEM.fire('question_note_stop_follow');return false;\"\u003e\u003ca href=\"#\"\u003eStop Following\u003c/a\u003e\u003c/li\u003e\n            {{else}}\n              \u003cli onclick=\"PEM.fire('question_note_start_follow');return false;\"\u003e\u003ca href=\"#\"\u003eFollow\u003c/a\u003e\u003c/li\u003e\n            {{/if}}\n            {{#if content.canEdit}}\n            {{#unless content.isPoll}}\n            \u003c!--\u003cli onclick=\"PEM.fire('attach_file');return false;\"\u003e\u003ca href=\"#\"\u003eAttach File\u003c/a\u003e\u003c/li\u003e--\u003e\n            \u003cli onclick=\"PEM.fire('question_note_convert_type');return false;\"\u003e\u003ca href=\"#\"\u003e{{#if content.isQuestion}}Convert Question to Note{{else}}Convert Note to Question{{/if}}\u003c/a\u003e\u003c/li\u003e\n            {{/unless}}\n            {{#if content.isQuestion}}\n            \u003cli onclick=\"PEM.fire('question_mark_duplicate');return false;\"\u003e\u003ca href=\"#\"\u003eMark as Duplicate\u003c/a\u003e\u003c/li\u003e\n            {{/if}}\n            {{#unless content.isPoll}}\n            \u003cli onclick=\"P.note_view.startEdit();return false;\"\u003e\u003ca href=\"#\"\u003eChange Visibility of Post\u003c/a\u003e\u003c/li\u003e\n            {{#if user.isInst}}\n            \u003cli onclick=\"P.note_view.startEdit();return false;\"\u003e\u003ca href=\"#\"\u003eAdd to Reading List\u003c/a\u003e\u003c/li\u003e\n            {{/if}}\n            {{/unless}}\n            {{/if}}\n            \u003c!--\n            \u003cli onclick=\"PEM.fire('mark_as_duplicate');return false;\"\u003e\u003ca href=\"#\"\u003eMark as Duplicate\u003c/a\u003e\u003c/li\u003e\n          --\u003e\n            {{#if content.canEdit}}\n            \u003cli onclick=\"PEM.fire('report_spam');return false;\"\u003e\u003ca href=\"#\"\u003eFlag as Inappropriate\u003c/a\u003e\u003c/li\u003e\n            {{/if}}\n            {{#if content.canDelete}}\n              \u003cli onclick=\"return PEM.fire('delete_content');\"\u003e\u003ca href=\"#\"\u003eDelete {{content.type}}\u003c/a\u003e\u003c/li\u003e\n            {{/if}}\n            {{#if user.isInst}} {{#unless content.disabledHistory}}\n            \u003cli\u003e\u003ca href=\"#\" onclick=\"return P.history_slider.toggleBan();\" \u003e\u003cspan class=\"userBannerText\"\u003eOpen Ban User Console\u003c/span\u003e\u003c/a\u003e\u003c/li\u003e\n            {{/unless}}{{/if}}\n          \u003c/ul\u003e\n        \u003c/div\u003e\n      \u003c/div\u003e\n      {{/unless}}\n      {{/unless}}\n      \u003cdiv class=\"post_region_content note\" id=\"view_quesiton_note\" oncopy=\"piazzaCopy(event, P.note_view.content.othersCopy);\"\u003e\n        {{#questionNoteText content network}}{{/questionNoteText}}\n      \u003c/div\u003e\n    \u003c/div\u003e\n    \u003cdiv class=\"post_region_content edit_mode\" style=\"display:none;\" id=\"edit_question_note\"\u003e\n      \u003ch3\u003eSummary\u003c/h3\u003e\n      \u003cinput class=\"post_region_input\" id=\"edit_question_note_subject\" placeholder=\"Post Summary\"/\u003e\n      \u003cdiv class=\"error_message no_summary\"\u003ePlease enter a brief summary of your post.\u003c/div\u003e\n      \u003cdiv class=\"error_message long_summary\"\u003ePlease enter a summary with no more than 100 characters.\u003c/div\u003e\n      {{#if content.isPoll}}\n        \u003cdiv class=\"error_message\"\u003e\n            Introducing new poll options in the middle or re-ordering poll options will interfere with any existing poll results.\n        \u003c/div\u003e\n      {{/if}}\n      \u003ch3\u003eDetails\u003c/h3\u003e\n      \u003cdiv id=\"question_note_editor\"\u003e\u003c/div\u003e\n      \u003cdiv class=\"error_message no_details\"\u003ePlease provide some details to better explain your question.\u003c/div\u003e\n\n      \u003cdiv class=\"post_edit_meta_wrapper\" style=\"display:block\"\u003e\n        {{#unless network.config.disable_folders}}\n        \u003cdiv class=\"post_edit_meta clearFix\"\u003e\n          \u003clabel\u003eFolders:\u003c/label\u003e\n          \u003cul id=\"edit_quesiton_note_folders\"\u003e\n            {{#folder_selector}}{{/folder_selector}}\n          \u003c/ul\u003e\n        \u003c/div\u003e\n        \u003cdiv class=\"error_message no_folders\"\u003ePlease select at least one folder for your post.\u003c/div\u003e\n        {{/unless}}\n        {{#unless content.isPoll}}\n          \u003cdiv class=\"post_edit_meta clearFix\" id=\"content_type_selector\"\u003e\n              \u003clabel\u003eSave as:\u003c/label\u003e\n              \u003cinput type=\"radio\" name=\"question_type\" id=\"question_type_0\" value=\"Question\" {{#if content.isQuestion}}checked{{/if}} /\u003e\u003clabel for=\"question_type_0\"\u003e  Question  (this needs an answer)\u003c/label\u003e\n              \u003cinput type=\"radio\" name=\"question_type\" id=\"question_type_1\" value=\"Note\" {{#if content.isNote}}checked{{/if}}/\u003e\u003clabel for=\"question_type_1\"\u003e  Note  (this doesn't need an answer)\u003c/label\u003e\n          \u003c/div\u003e\n        {{/unless}}\n\n        \u003cdiv class=\"post_edit_meta clearFix\" id=\"content_type_selector\"\u003e\n            \u003clabel\u003eVisible to:\u003c/label\u003e\n            \u003cinput type=\"radio\" name=\"question-visibility\" id=\"question_visibility_0\" onchange=\"P.note_view.visibilityChange()\" value=\"entire_class\" {{#if content.isPublic}}checked{{/if}} {{#if network.privatePostsOnly}}disabled{{/if}}/\u003e\u003clabel for=\"question_visibility_0\" style=\"font-weight: bold;\"\u003e\u0026nbsp;Entire class{{#if network.privatePostsOnly}} (disabled){{/if}}\u003c/label\u003e\u0026nbsp;\u0026nbsp;\n            {{#if network.subGroups}}\n              \u003cinput type=\"radio\" name=\"question-visibility\" id=\"question_visibility_1\" onchange=\"P.note_view.visibilityChange()\" class=\"edit-sections\" value=\"sections\" {{#if content.isGroup}}checked{{/if}}/\u003e\u003clabel for=\"question_visibility_1\" style=\"font-weight: bold;\"\u003e\u0026nbsp;Group\u003c/label\u003e\u0026nbsp;\u0026nbsp;\n            {{/if}}\n            {{#unless network.noPrivatePosts}}\n            \u003cinput type=\"radio\" name=\"question-visibility\" id=\"question_visibility_2\" onchange=\"P.note_view.visibilityChange()\" value=\"private\" {{#if content.isPrivateAndNotGroup}}checked{{/if}}/\u003e\n            \u003clabel for=\"question_visibility_2\" class=\"\" style=\"font-weight: bold;margin-right: 0px;\"\u003e\n              \u0026nbsp;Private\n            \u003c/label\u003e\n            \u003cspan style=\"font-weight: normal;\"\u003e\n              {{#unless user.isInst}}\n              {{#if content.visibleOne}}\n              \u003clabel style=\"font-weight:normal;\"\u003eto {{#toUserName content.visibleOne \"me\"}}{{/toUserName}}\n                {{#if content.visibleTwo}}\n                and {{#toUserName content.visibleTwo \"me\"}}{{/toUserName}}\n                {{/if}}\n                {{#if content.visibleArr}}\n                  {{#showOthers content.visibleArr}}{{/showOthers}}\n                {{/if}}\n              \u003c/label\u003e\n              {{else}}\n              \u003clabel style=\"font-weight:normal;\"\u003e to Individual Student(s) / Instructor(s)\u003c/label\u003e\n              {{/if}}\n              {{/unless}}\n              {{#if user.isInst}}\n              \u003clabel style=\"font-weight:normal;\"\u003e to Individual Student(s) / Instructor(s)\u003c/label\u003e\n              {{/if}}\n            {{/unless}}\n            \u003c/span\u003e\n        \u003c/div\u003e\n\n        \u003cdiv class=\"edit_post_names\" style=\"margin-left: 70px;margin-top: 6px;\"\u003e\n          \u003cdiv class=\"new_post_error too_many_names\"\u003eYou can send individual posts to up to 10 people\u003c/div\u003e\n          \u003cinput id=\"member_names\" class=\"new_post_textarea\" type=\"text\" placeholder=\"Enter one or more names...\"  style=\"width: 400px;height: 16px;\"\u003e\n          \u003cdiv id=\"member_names_message\" class=\"small_grey_text\"\u003eType \"Instructors\" to include all instructors.\u003c/div\u003e\n          \u003cdiv class=\"new_post_error no_names\"\u003eEnter at least one name before submitting this post\u003c/div\u003e\n          \u003cul class=\"dropdown-menu user_list general_dropdown\" id=\"member_names_dropdown_list\" role=\"menu\" aria-labelledby=\"dropdownMenu\"\u003e\u003c/ul\u003e\n          \u003cul class=\"posting_meta_section_list selected_user_list clearfix\"\u003e\u003c/ul\u003e\n        \u003c/div\u003e\n\n        {{#if network.subGroups}}\n        \u003cdiv id=\"subgroup_list_edit\" style='display:none;margin-top:5px;margin-left:5px;'\u003e\n          \u003cselect id=\"subgroup_dropdown_edit\" class=\"anonymity_selector\" onchange=\"P.note_view.subGroupChange()\"\u003e\n            {{#if network.showAllGroups}}\n            \u003coption value='All my groups'\u003eAll my groups\u003c/option\u003e\n            {{/if}}\n            {{#each network.subGroups}}\n            \u003coption value='{{this.id}}'\u003e{{this.name}}\u003c/option\u003e\n            {{/each}}\n          \u003c/select\u003e\n          \u003cdiv class=\"error_wrapper\" id=\"not-in-section-student-edit\" style=\"display: none;\"\u003e\n            \u003cdiv class=\"error main\"\u003eYou are currently not in this group\u003c/div\u003e\n            \u003cdiv class=\"error sub\"\u003ePlease ask your instructor to enroll you to this group on Piazza\u003c/div\u003e\n          \u003c/div\u003e\n          \u003cdiv class=\"error_wrapper\" id=\"not-in-section-can-join-edit\" style=\"display: none;\"\u003e\n            \u003cdiv class=\"error main\"\u003eYou are currently not in this group\u003c/div\u003e\n            \u003cdiv class=\"error sub\"\u003eYou have to join this group to post\u003c/div\u003e\n          \u003c/div\u003e\n          \u003cdiv class=\"error_wrapper\" id=\"not-in-section-instructor-edit\" style=\"display: none;\"\u003e\n            \u003cdiv class=\"error main\"\u003eYou are currently not in this group\u003c/div\u003e\n            \u003cdiv class=\"error sub\"\u003eYou will not see this post in your feed\u003c/div\u003e\n          \u003c/div\u003e\n        \u003c/div\u003e\n        {{/if}}\n\n        {{#ifEquals content.type \"poll\"}}\n        \u003cdiv class=\"posting_meta_section show_for_poll poll_choices clearFix\"\u003e\n          \u003cdiv class=\"left_section_edit main_label\"\u003ePoll Choices\u003c/div\u003e\n          \u003cdiv class=\"right_section\"\u003e\n            \u003cdiv class=\"input-append\"\u003e\n              \u003cinput class=\"span3\" id=\"input_poll_choice\" placeholder=\"Enter poll choices...\" type=\"text\" \u003e\n              \u003cbutton class=\"btn\" type=\"button\" id=\"add_poll_choice\"\u003eAdd\u003c/button\u003e\n            \u003c/div\u003e\n            \u003cdiv class=\"new_post_error no_poll_choices\"\u003ePlease enter poll choices before submitting...\u003c/div\u003e\n            \u003cul id=\"poll_options_preview\" class=\"poll_options\"\u003e\n            \u003c/ul\u003e\n          \u003c/div\u003e\n        \u003c/div\u003e\n\n        \u003cdiv class=\"posting_meta_section post_summary clearFix show_for_poll\"\u003e\n          \u003cdiv class=\"left_section_edit main_label\"\u003e\n            Poll Explanation\n            \u003csmall\u003e(Elaborate on what the correct answer is, or share other details)\u003c/smalL\u003e\n          \u003c/div\u003e\n          \u003cdiv class=\"right_section\"\u003e\n            \u003cinput type=\"text\" id=\"input_poll_explanation\" class=\"span3\" placeholder=\"Enter an explanation...\"/\u003e\u003cspan class=\"small_grey_text\"\u003e\u0026nbsp;This is optional, and it will be displayed when the poll closes.\u003c/span\u003e\n\n          \u003c/div\u003e\n        \u003c/div\u003e\n\n        \u003cdiv class=\"posting_meta_section show_for_poll selections_allowed clearFix\"\u003e\n          \u003cdiv class=\"left_section_edit main_label\"\u003ePoll Selection Type\u003c/div\u003e\n          \u003cdiv class=\"right_section\"\u003e\n            \u003clabel class=\"radio one_line\"\u003e\n              \u003cinput type=\"radio\" name=\"selections_allowed\" id=\"selections_allowed_one\" value=\"option1\" {{#unless multiple_selections}}checked{{/unless}}\u003e\n              One choice allowed\n            \u003c/label\u003e\n            \u003clabel class=\"radio one_line\"\u003e\n              \u003cinput type=\"radio\" name=\"selections_allowed\" id=\"selections_allowed_more\" value=\"option2\" {{#if multiple_selections}}checked{{/if}}\u003e\n              Multiple choices allowed\n            \u003c/label\u003e\n          \u003c/div\u003e\n        \u003c/div\u003e\n        \u003cdiv class=\"posting_meta_section show_for_poll poll_close_date clearFix\"\u003e\n          \u003cdiv class=\"left_section_edit main_label\"\u003ePoll Close Date\u003cbr/\u003e\n          \u003c/div\u003e\n          \u003cdiv class=\"right_section\"\u003e\n            \u003clabel class=\"radio\"\u003e\n              \u003cinput type=\"radio\" name=\"poll_close_date\" id=\"poll_close_date_after\" value=\"option1\" {{#unless always_open}}checked{{/unless}}\u003e\u003c/input\u003e\n              \u003cspan\u003eAfter\u003c/span\u003e\n            \u003c/label\u003e\n            \u003cselect class=\"short\" id=\"poll_close_date_menu\"\u003e\n              \u003coption value=\"2\" {{#ifEquals close_after \"2\"}}selected=\"selected\"{{/ifEquals}}\u003e2 hours\u003c/option\u003e\n              \u003coption value=\"6\" {{#ifEquals close_after \"6\"}}selected=\"selected\"{{/ifEquals}}\u003e6 hours\u003c/option\u003e\n              \u003coption value=\"24\" {{#ifEquals close_after \"24\"}}selected=\"selected\"{{/ifEquals}}\u003e1 day\u003c/option\u003e\n              \u003coption value=\"48\" {{#ifEquals close_after \"48\"}}selected=\"selected\"{{/ifEquals}}\u003e2 days\u003c/option\u003e\n              \u003coption value=\"168\" {{#ifEquals close_after \"168\"}}selected=\"selected\"{{/ifEquals}}\u003e7 days\u003c/option\u003e\n            \u003c/select\u003e\n            \u003cspan class=\"small_grey_text\"\u003eYou can close the poll at any time (useful for in-class polls)\u003c/span\u003e\n            \u003clabel class=\"radio one_line\"\u003e\n              \u003cinput type=\"radio\" name=\"poll_close_date\" id=\"poll_close_date_never\" value=\"option2\" {{#if always_open}}checked{{/if}}\u003e\n              Always open\n            \u003c/label\u003e\n          \u003c/div\u003e\n        \u003c/div\u003e\n        \u003cdiv class=\"posting_meta_section show_for_poll show_poll_results clearFix\"\u003e\n          \u003cdiv class=\"left_section_edit main_label\"\u003eShow Results to Students\u003c/div\u003e\n          \u003cdiv class=\"right_section\"\u003e\n            \u003clabel class=\"radio one_line\"\u003e\n              \u003cinput type=\"radio\" name=\"show_poll_results\" id=\"show_poll_results_before_member_votes\" value=\"option1\" {{#ifEquals show_results \"\"}} checked {{/ifEquals}}\u003e\n              Before a student votes\n            \u003c/label\u003e\n            \u003clabel class=\"radio one_line\"\u003e\n              \u003cinput type=\"radio\" name=\"show_poll_results\" id=\"show_poll_results_after_member_votes\" value=\"option2\" {{#ifEquals show_results \"av\"}} checked {{/ifEquals}}\u003e\n              After a student votes\n            \u003c/label\u003e\n            \u003clabel class=\"radio one_line\"\u003e\n              \u003cinput type=\"radio\" name=\"show_poll_results\" id=\"show_poll_results_after_poll_closes\" value=\"option3\" {{#ifEquals show_results \"ac\"}} checked {{/ifEquals}}\u003e\n              After poll closes\n              \u003cspan class=\"small_grey_text\"\u003e(instructors will always see results in real time)\u003c/span\u003e\n            \u003c/label\u003e\n            \u003clabel class=\"radio one_line\"\u003e\n              \u003cinput type=\"radio\" name=\"show_poll_results\" id=\"show_poll_results_keep_private\" value=\"option4\" {{#ifEquals show_results \"nn\"}} checked {{/ifEquals}}\u003e\n              Never show results to students\n              \u003cspan class=\"small_grey_text\"\u003e(instructors will always have the option to view results at any time)\u003c/span\u003e\n            \u003c/label\u003e\n          \u003c/div\u003e\n        \u003c/div\u003e\n        \u003cdiv class=\"posting_meta_section show_for_poll revotes clearFix\"\u003e\n          \u003cdiv class=\"left_section_edit main_label\"\u003eRevotes allowed?\u003cbr/\u003e\n            \u003csmall\u003eVoters will be able to revote as many times as they want\u003c/small\u003e\n          \u003c/div\u003e\n          \u003cdiv class=\"right_section\"\u003e\n            \u003clabel class=\"radio one_line\"\u003e\n              \u003cinput type=\"radio\" name=\"revotes\" id=\"revotes_yes\" value=\"option1\" {{#if revote }} checked {{/if}}\u003e\n              Yes\n            \u003c/label\u003e\n            \u003clabel class=\"radio one_line\"\u003e\n              \u003cinput type=\"radio\" name=\"revotes\" id=\"revotes_no\" value=\"option2\" {{#unless revote }}checked {{/unless}}\u003e\n              No\n            \u003c/label\u003e\n          \u003c/div\u003e\n        \u003c/div\u003e\n\n        \u003cdiv class=\"posting_meta_section show_for_poll poll_anonymity clearFix\"\u003e\n          \u003cdiv class=\"left_section_edit main_label\"\u003ePoll Anonymity\u003c/div\u003e\n          \u003cdiv class=\"right_section\"\u003e\n            {{#unless network.isNoVerify}}\n            \u003clabel class=\"radio one_line\"\u003e\n              \u003cinput type=\"radio\" name=\"poll_anonymity\" id=\"poll_anonymity_everyone\" value=\"option1\" {{#ifEquals anonymity \"no\"}} checked {{/ifEquals}}\u003e\n              Show names to everyone\n            \u003c/label\u003e\n            \u003clabel class=\"radio one_line\"\u003e\n              \u003cinput type=\"radio\" name=\"poll_anonymity\" id=\"poll_anonymity_students\" value=\"option1\" {{#ifEquals anonymity \"stud\"}} checked {{/ifEquals}}\u003e\n              Show names to instructors but not students\n            \u003c/label\u003e\n            {{/unless}}\n            \u003clabel class=\"radio one_line\"\u003e\n              \u003cinput type=\"radio\" name=\"poll_anonymity\" id=\"poll_anonymity_nobody\" value=\"option2\" {{#ifEquals anonymity \"yes\"}} checked {{/ifEquals}}\u003e\n              Don't show names to anyone\n            \u003c/label\u003e\n          \u003c/div\u003e\n        \u003c/div\u003e\n        {{/ifEquals}}\n\n        {{#if show_must_read}}\n        \u003cdiv class=\"post_edit_meta clearFix\" id=\"must_read_div\"\u003e\n          \u003clabel\u003eReading list:\u003c/label\u003e\n          \u003cinput type=\"checkbox\" id=\"must_read\" style=\"margin-bottom: 5px\" {{#if content.config.must_read}}checked{{/if}} onchange=\"P.note_view.onMustReadChange()\"\u003e\u003clabel\u003e Add this post to students’ reading lists\u003c/label\u003e\n          \u003cdiv class=\"post_edit_meta clearFix\" id=\"must_read_details\" style=\"margin-left: 95px;{{#unless content.config.must_read}}display:none{{/unless}}\"\u003e\n            \u003clabel\u003eRemove from reading list \u003c/label\u003e\n            \u003cinput type=\"radio\" class=\"must_read_expire_radio\" name=\"must_read_expire_radio\" id=\"must_read_manual\" {{#unless content.config.must_read_expire}}checked{{/unless}}\u003e\u003clabel\u003e Manually \u003c/label\u003e\n            \u003cinput type=\"radio\" class=\"must_read_expire_radio\" name=\"must_read_expire_radio\" id=\"must_read_auto\" {{#if content.config.must_read_expire}}checked{{/if}}\u003e\u003clabel\u003e Schedule \u003c/label\u003e\n            \u003cinput type=\"text\" id=\"must_read_date\" style=\"width: 100px;margin-bottom: 4px;height: 10px;\"\u003e\n            \u003cinput type=\"text\" id=\"must_read_time\" style=\"width: 70px;margin-bottom: 4px;height: 10px;\"\u003e \u003clabel\u003e{{ timezone }}\u003c/label\u003e\n            \u003cbr\u003e\n            \u003cinput type=\"checkbox\" id=\"must_read_to_post\" style=\"margin-bottom: 5px\" {{#if content.config.must_read_to_post}}checked{{/if}}\u003e\u003clabel\u003e Require students to have read this post before they can create a new post\u003c/label\u003e\u003cbr\u003e\n          \u003c/div\u003e\n        \u003c/div\u003e\n        {{/if}}\n\n      \u003c/div\u003e\n    \u003c/div\u003e\n    \u003cdiv class=\"post_region_message_wrapper currently_editing\" style=\"display:none;margin-top:0px;\"\u003e\n      \u003cdiv class=\"post_region_message warning show\"\u003e\n        \u003cspan class=\"editing_message\"\u003e\u003c/span\u003e\n      \u003c/div\u003e\n    \u003c/div\u003e\n    \u003cdiv class=\"post_region_actions note\" id=\"view_question_note_bar\"\u003e\n      {{#ifEquals content.subject \"Intro to the Piazza Network\"}}\n        \u003cspan style=\"color:#999;\"\u003eStill have more questions about Piazza Network? Send us an email at \u003ca href=\"mailto:questions@piazza.com\"\u003equestions@piazza.com\u003c/a\u003e.\u003c/span\u003e\n      {{else}}\n      {{#if content.canEdit}}\n        \u003ca data-pats=\"edit_button\" class=\"post_action edit main btn btn-primary btn-mini\" tutorial=\"Improve this question to add more details or clarify something.\" href=\"#\"\u003eedit\u003c/a\u003e\n        \u003cspan class=\"middot\"\u003e\u0026middot;\u003c/span\u003e\n        \u003ca data-pats=\"mark_post_good\" class=\"post_action do_good_note {{#if content.is_tag_good}}hide{{/if}}\" onclick=\"PEM.fire('do_good_post'); return false;\" href=\"#\"\u003egood {{content.type}}\u003c/a\u003e\n        \u003ca data-pats=\"unmark_post_good\" class=\"post_action undo_good_note {{#unless content.is_tag_good}}hide{{/unless}}\" onclick=\"PEM.fire('undo_good_post'); return false;\" href=\"#\"\u003eundo good {{content.type}}\u003c/a\u003e\n        \u003cspan data-pats=\"mark_post_count\" class=\"post_actions_number good_note\"\u003e{{#if content.tag_good}}{{content.tag_good.length}}{{else}}0{{/if}}\u003c/span\u003e\n      {{/if}}\n      {{/ifEquals}}\n      \u003cdiv data-pats=\"updated_text\" class=\"post_region_actions_meta\"\u003eUpdated {{#toRelativeTime content.last_edit}}{{/toRelativeTime}} by\n      {{#if content.showMyAnon}}\n        \u003cspan class=\"private_poster highlight\"\u003e{{#toUserName user.id \"stud\" false content.firstUidA}}{{/toUserName}}\u003c/span\u003e\n      {{else}}\n        {{#if content.lastSet}}\n          {{#toUserName content.lastUid content.anon false content.lastUidA}}{{/toUserName}}\n        {{else}}\n          \u003cspan class=\"private_poster highlight\"\u003eAnonymous\u003c/span\u003e\n        {{/if}}\n      {{/if}}\n\n      {{#showOthers content.others content.othersAnon content.othersA}}{{/showOthers}}\n      \u003c/div\u003e\n    \u003c/div\u003e\n    \u003cdiv class=\"post_region_actions edit_mode\" id=\"edit_question_note_bar\"\u003e\n      \u003ca data-pats=\"submit_button\" class=\"post_action submit main btn btn-primary btn-mini submit_post\" href=\"#\"\u003esubmit\u003c/a\u003e\n      {{#if anon}}\n        \u003cspan class=\"post_action\"\u003e\n       \u0026nbsp;\u0026nbsp;as \u003cselect class=\"anonymity_selector\" id=\"edit_quesiton_anonymity\"\u003e\n          {{#each anon}}\n          \u003coption value=\"{{this.val}}\"\u003e{{this.title}}\u003c/option\u003e\n          {{/each}}\n        \u003c/select\u003e\n        \u003c/span\u003e\n      {{/if}}\n      \u003ca id=\"autosave_button\" class=\"post_action save_draft btn btn-secondary btn-mini\" href=\"#\" style=\"margin: 0;margin-left:3px;\" onclick=\"return P.text_editor.autoSave('question_note_edit', true)\"\u003esave draft\u003c/a\u003e\n      \u003cspan class=\"middot\"\u003e\u0026middot;\u003c/span\u003e\n      \u003ca class=\"post_action preview preview_link\" id=\"preview_link_question_note_edit\" href=\"#\" onclick=\"return P.text_editor.handlePreviewButton('question_note_edit', true);\"\u003epreview\u003c/a\u003e\n      \u003ca data-pats=\"cancel_button\" class=\"post_action cancel right\" href=\"#\" onclick=\"PEM.fire('cancelOne', 'question_note_edit');return false;\"\u003ecancel\u003c/a\u003e\n    \u003c/div\u003e\n");</script>

  <script type="text/javascript">
PTM.createTemplate("question_note_view_text", "{{#unless content.isTeammateSearch}}\n\u003ch1 data-pats=\"question_title_text\" class=\"post_region_title\"\u003e{{{content.subject}}}\u003c/h1\u003e\n{{/unless}}\n\u003cdiv class=\"post_region_text\" id=\"questionText\"\u003e{{#processText content.body content.type}}{{/processText}}\u003cbr/\u003e\u003c/div\u003e\n\u003cdiv data-pats=\"folders\" class=\"post_region_folders\"\u003e\n  \u003cspan\u003e\n    {{#folderlist content.folders network.folders}}{{/folderlist}}\n  \u003c/span\u003e\n\u003c/div\u003e\n\u003cdiv class=\"attachments\" {{#unless content.attach}}style=\"display:none;\"{{/unless}}\u003e\n  \u003cdiv class=\"attachments_divider\"\u003e\u003c/div\u003e\n  \u003cdiv class=\"attachments_count\"\u003eAttachments:\u003c/div\u003e\n  \u003ctable class=\"attachments_list\"\u003e\n    {{#each content.attach}}\n      {{#attachment this ../content ../network}}{{/attachment}}\n    {{/each}}\n  \u003c/table\u003e\n\u003c/div\u003e\n\u003cdiv class=\"post_region_message_wrapper\"\u003e\n  \u003cdiv id=\"endorse_text\"\u003e{{#endorseText content}}{{/endorseText}}\u003c/div\u003e\n  {{#if content.visibleOne}}\n    {{#ifEquals content.type \"poll\"}}\n       {{#ifEquals content.config.unpublished 1}}\n          \u003cdiv class=\"post_region_message show\"\u003eThis poll is not yet published and only visible to \u003cspan class=\"private_poster highlight\"\u003e{{#toUserName content.visibleOne}}{{/toUserName}}\u003c/span\u003e\n       {{else}}\n          \u003cdiv class=\"post_region_message show\"\u003eThis poll is published and only visible to \u003cspan class=\"private_poster highlight\"\u003e{{#toUserName content.visibleOne}}{{/toUserName}}\u003c/span\u003e\n       {{/ifEquals}}\n       {{#if content.visibleTwo}}\n          and \u003cspan class=\"private_poster highlight\"\u003e{{#toUserName content.visibleTwo}}{{/toUserName}}\u003c/span\u003e\u003c/div\u003e\n       {{/if}}\n       {{#if content.visibleArr}}\n          {{#showOthers content.visibleArr}}{{/showOthers}}\u003c/div\u003e\n       {{/if}}\n    {{else}}\n  \u003cdiv class=\"post_region_message show\"\u003eThis private post is only visible to \u003cspan class=\"private_poster highlight\"\u003e{{#toUserName content.visibleOne}}{{/toUserName}}\u003c/span\u003e\n    {{#if content.visibleTwo}}\n    and \u003cspan class=\"private_poster highlight\"\u003e{{#toUserName content.visibleTwo}}{{/toUserName}}\u003c/span\u003e\n    {{/if}}\n    {{#if content.visibleArr}}\n      {{#showOthers content.visibleArr}}{{/showOthers}}\n    {{/if}}\n    {{#if content.canDelete}}\n      \u003cbr/\u003e\u003cspan\u003e(Change this private post into an anonymous public post under Actions \u003e Change Visibility of Post)\u003c/span\u003e\n    {{/if}}\n  \u003c/div\u003e\n\n    {{/ifEquals}}\n  {{else}}\n  {{#if content.isPrivateAndNotGroup}}\n  \u003cdiv class=\"post_region_message show\"\u003e\n    This private post is only visible to \u003cspan class=\"private_poster highlight\"\u003eoriginal poster\u003c/span\u003e and \u003cspan class=\"private_poster highlight\"\u003eInstructors\u003c/span\u003e\n    {{#if content.canDelete}}\n      \u003cbr/\u003e\u003cspan\u003e(Change this private post into an anonymous public post under Actions \u003e Change Visibility of Post)\u003c/span\u003e\n    {{/if}}\n  \u003c/div\u003e\n  {{/if}}\n  {{/if}}\n");</script>

  <script type="text/javascript">
PTM.createTemplate("question_note_view_endorse_text", "{{#if content.endorseOne}}\n\u003cdiv class=\"post_region_message endorse show\"\u003e\n  \u003cspan class=\"endorse_message\"\u003e{{#toUserName content.endorseOne false}}{{/toUserName}}\n  {{#if content.endorseTwo}}\n  and {{#toUserName content.endorseTwo false}}{{/toUserName}}\n  {{/if}}\n  {{#if content.endorseArr}}\n    {{#showOthers content.endorseArr}}{{/showOthers}}\n  {{/if}}\n  thought this was a good {{content.showType}}\u003c/span\u003e\n\u003c/div\u003e\n{{/if}}");</script>

  <script type="text/javascript">
PTM.createTemplate("question_note_view_endorse_class", "{{#if content.endorseOne}}\n\u003cdiv class=\"post_region_message endorse show\"\u003e\n  \u003cspan class=\"endorse_message\"\u003e~\u0026#160;An instructor ({{#toUserName content.endorseOne false}}{{/toUserName}})\n  {{#if content.isAnswer}}\n  endorsed this {{content.showType}}\n  {{else}}\n  thinks this is a good {{content.showType}}\n  {{/if}}\n  \u0026#160;~\u003c/span\u003e\n\u003c/div\u003e\n{{/if}}");</script>

  <script type="text/javascript">
PTM.createTemplate("attachment", "\u003ctr class=\"attachments_item\" id=\"attach_{{attach.id}}\"\u003e\n  \u003ctd class=\"preview\"\u003e\u003cimg src=\"{{#extensionIcon attach.name}}{{/extensionIcon}}\"/\u003e\u003c/td\u003e\n  \u003ctd class=\"info\"\u003e\n    \u003cdiv class=\"attach_wrapper\"\u003e\n      \u003cdiv class=\"title\"\u003e{{attach.name}}\u003c/div\u003e\n      \u003cdiv class=\"other\"\u003e\n        \u003cspan class=\"size\"\u003e{{attach.size}}\u003c/span\u003e\n        \u003ca href=\"https://cdn-uploads.piazza.com/attach/{{network.id}}/{{content.id}}/{{attach.id}}/{{attach.name}}\" target=\"_blank\"\u003eOpen\u003c/a\u003e\n        {{#isImage attach.name}}\n          \u003ca href=\"#\" id=\"m_{{attach.id}}\" onclick=\"$('#m_{{attach.id}}').hide();$('#l_{{attach.id}}').show();return false;\"\u003eEmbed this image inline\u003c/a\u003e\n          \u003cdiv id=\"l_{{attach.id}}\" style=\"display:none;\"\u003e\n            \u003cb\u003ePaste this in image url:\u003c/b\u003e https://cdn-uploads.piazza.com/attach/{{network.id}}/{{content.id}}/{{attach.id}}/{{attach.name}}\n          \u003c/div\u003e\n        {{/isImage}}\n        \u003ca href=\"#\" target=\"_blank\" onclick=\"PEM.fire('deleteAttachment', '{{attach.id}}');return false;\"\u003eDelete\u003c/a\u003e\n      \u003c/div\u003e\n    \u003c/div\u003e\n  \u003c/td\u003e\n\u003c/tr\u003e\n");</script>

  <script type="text/javascript">
PTM.createModuleTemplate("homescreen", "homescreen_instructor", "\u003cdiv class=\"homescreen\"\u003e\n\n  \u003cdiv class=\"live_qa\" style=\"display:none;\"\u003e\n    \u003cdiv style=\"height:30px;\"\u003e\n      \u003cspan class=\"pull-right\" style=\"margin-top: 8px;font-size: 12px;\" \u003e\n        \u003ca href=\"#\" onclick=\"P.feed.filterFeed();return false;\"\u003eExit Live Q\u0026A\u003c/a\u003e\n      \u003c/span\u003e\n    \u003c/div\u003e\n    \u003cdiv class=\"homescreen_box clearfix\"\u003e\n      \u003cdiv class=\"prof_message clearfix\" style=\"margin-top:0px;\"\u003e\n        \u003ch1 style=\"margin-bottom:15px;\"\u003ePiazza ‘Live Q\u0026A’ Feature\u003c/h1\u003e\n        \u003cp\u003eLive Q\u0026A allows instructors to easily see and address the questions that students most care about during a live lecture or office hour session. You can host Live Q\u0026A sessions with distinct start and end times, during which students can ask questions and upvote one another’s questions. Questions asked during a Live Q\u0026A session are stored under your Live Q\u0026A feed so as not to clutter your primary class Q\u0026A feed.\u003c/p\u003e\n        \u003cp\u003eTo learn more about how Live Q\u0026A works, visit our \u003ca href=\"https://support.piazza.com/support/solutions/folders/48000670520\" target=\"_blank\"\u003eLive Q\u0026A support documentation\u003c/a\u003e.\u003c/p\u003e\n          \u003cbutton class=\"live_button_start\" onclick=\"$('#live_max_hours').show();event.stopPropagation();\"\u003eStart Live Q\u0026A Now\u003c/button\u003e\n          \u003cdiv class=\"dropdown-menu general_dropdown\" id=\"live_max_hours\" style=\"padding: 10px 10px; width: 280px; font-weight: normal;margin-top: -15px;margin-left: 15px;\" onclick=\"event.stopPropagation();\"\u003e\n            Host Live Q\u0026A for (you can manually stop it any time):\n            \u003clabel style=\"margin-bottom: 0px; line-height: 16px;\"\u003e\n              \u003cinput type=\"radio\" name=\"maxLiveRadio\" id=\"max_live_1\" checked\u003e\n              \u003cspan style=\"vertical-align: sub\"\u003e 1 hour\u003c/span\u003e\n            \u003c/label\u003e\n            \u003clabel style=\"margin-bottom: 0px; line-height: 16px;\"\u003e\n              \u003cinput type=\"radio\" name=\"maxLiveRadio\" id=\"max_live_2\"\u003e\n              \u003cspan style=\"vertical-align: sub\"\u003e 2 hours\u003c/span\u003e\n            \u003c/label\u003e\n            \u003clabel style=\"margin-bottom: 0px; line-height: 16px;\"\u003e\n              \u003cinput type=\"radio\" name=\"maxLiveRadio\" id=\"max_live_3\"\u003e\n              \u003cspan style=\"vertical-align: sub\"\u003e 3 hours\u003c/span\u003e\n            \u003c/label\u003e\n            \u003clabel style=\"margin-bottom: 0px; line-height: 16px;\"\u003e\n              \u003cinput type=\"radio\" name=\"maxLiveRadio\" id=\"max_live_4\"\u003e\n              \u003cspan style=\"vertical-align: sub\"\u003e 1 day\u003c/span\u003e\n            \u003c/label\u003e\n            \u003clabel style=\"margin-bottom: 5px; line-height: 16px;\"\u003e\n              \u003cinput type=\"radio\" name=\"maxLiveRadio\" id=\"max_live_5\"\u003e\n              \u003cspan style=\"vertical-align: sub\"\u003e 7 days\u003c/span\u003e\n            \u003c/label\u003e\n            \u003cdiv class=\"content-active\"\u003e\n              \u003cbutton class=\"btn-primary\" onclick=\"P.feed.startLive(true, '');closeDropDown();\"\u003eStart Live Q\u0026A\u003c/button\u003e\n              \u003ca href=\"\" style=\"display: inline-block; margin-left: 5px\" onclick=\"closeDropDown();return false;\"\u003ecancel\u003c/a\u003e\n            \u003c/div\u003e\n          \u003c/div\u003e\n          \u003cbutton class=\"live_button_stop\" style=\"display:none\" onclick=\"P.feed.startLive(false);return false;\"\u003eStop Live Q\u0026A\u003c/button\u003e\n          \u003cstrong class=\"live_on\" style=\"display:none; color:#333;\"\u003e(Live Q\u0026A in progress)\u003c/strong\u003e\n\n      \u003c/div\u003e\n    \u003c/div\u003e\n  \u003c/div\u003e\n\n  \u003cdiv class=\"homescreen_section_wrapper clearfix\" style=\"display:block;\" data-pats=\"instructor_homescreen\"\u003e\n\n    \u003c!-- Only for specific MOOCs --\u003e\n    {{#if network.config.allow_directory}}\n      \u003ca id=\"class_directory_link\" class=\"pull-right\" href=\"/class_directory/?nid={{network.id}}\"\u003eClass Directory\u003c/a\u003e\n    {{/if}}\n    \u003cdiv class=\"homescreen_section_header\" style=\"text-align: left;\"\u003e\n      \u003cdiv class=\"dropdown show-to-experienced premium-disabled-tipsy premium3\" style=\"display: inline-block; margin: 0 0 5px 10px; vertical-align: bottom;\"\u003e\n        \u003cbutton class=\"transparent premium-disabled-noclick\" onclick=\"$('#network_lock_menu').show();event.stopPropagation();\"\u003e\n          \u003cimg class=\"network-inactive\" src=\"/images/lock_icon.png\" alt=\"lock_icon\" width=\"15\" height=\"18\" /\u003e\n          \u003cimg class=\"network-active\" src=\"/images/unlock_icon.png\" alt=\"unlock_icon\" width=\"15\" height=\"18\" /\u003e\n          \u003cb class=\"\"\u003e\u0026blacktriangledown;\u003c/b\u003e\n        \u003c/button\u003e\n        \u003cul class=\"dropdown-menu general_dropdown\" id=\"network_lock_menu\" style=\"font-size: 12px; text-shadow: none;\"\u003e\n          \u003cli class=\"network-active dropdown-submenu\"\u003e\n            \u003ca \u003eLock this class immediately\u003c/a\u003e\n            \u003cul class=\"dropdown-menu\"\u003e\n              \u003cli\u003e\u003ca onclick=\"return P.homescreen.setState('inactive', false, false)\"\u003eLock this class, no posts allowed\u003c/a\u003e\u003c/li\u003e\n              \u003cli\u003e\u003ca onclick=\"return P.homescreen.setState('inactive', false, true)\"\u003eLock this class, block access to all posts\u003c/a\u003e\u003c/li\u003e\n              \u003cli\u003e\u003ca onclick=\"return P.homescreen.setState('inactive', true, false)\"\u003eLock this class, private posts to instructors allowed\u003c/a\u003e\u003c/li\u003e\n            \u003c/ul\u003e\n          \u003c/li\u003e\n          \u003cli class=\"network-inactive\"\u003e\n            \u003ca onclick=\"return P.homescreen.setState('active', false)\"\u003eUnlock this class now\u003c/a\u003e\n          \u003c/li\u003e\n          \u003cli\u003e\u003ca href=\"{{network.configLink}}\" target=\"_blank\"\u003eSchedule future lock dates/times\u003cbr\u003e\n              \u003cspan class=\"helper-text\" style=\"margin: 0; color: #999; font-size: 11px\"\u003e(Opens ‘Manage Class’ in a new tab)\u003c/span\u003e\n            \u003c/a\u003e\n          \u003c/li\u003e\n        \u003c/ul\u003e\n      \u003c/div\u003e\n       {{#if network.isGroup}}Activity Summary{{else}}Class at a Glance{{/if}}\n      \u003ci\u003e\n        \u003cspan\u003e\n          Updated\n          \u003cspan class=\"auto_update_time\" id=\"activity_last_updated\"\u003e\u003c/span\u003e.\n        \u003c/span\u003e\n        \u003cspan\u003e\n          \u003ca href=\"#\" onclick=\"P.homescreen.refreshInstructorStats(true);return false;\"\u003eReload\u003c/a\u003e\n        \u003c/span\u003e\n      \u003c/i\u003e\n      \u003cspan class=\"pull-right show-to-experienced\" style=\"margin-top: 8px;font-size: 12px;display:none;\" \u003e\n        \u003ca href=\"#\" id=\"homescreen_start_live\" onclick=\"P.feed.openLive();return false;\"\u003eGo to Live Q\u0026A\u003c/a\u003e\n        \u003cspan id=\"homescreen_start_live_progress\" style=\"display:none;\"\u003eLive Q\u0026A in progress\u003c/span\u003e\n      \u003c/span\u003e\n    \u003c/div\u003e\n    \u003cdiv class=\"homescreen_box clearfix\"\u003e\n      \u003cdiv class=\"clearfix\"\u003e\n        \u003cdiv class=\"homescreen_box_section\" style=\"width: 40%\"\u003e\n          \u003cdiv class=\"notification_wrapper clearfix\"\u003e\n            \u003cdiv class=\"notification_icon red\"\u003e\u003c/div\u003e\n            \u003cdiv class=\"notification_text\"\u003e\n              \u003cdiv class=\"strong_text\"\u003e\u003cspan id=\"new_question_count\"\u003e\u003c/span\u003e unread posts\u003c/div\u003e\n              \u003cdiv class=\"light_text\" id=\"view_new_questions\"\u003eclick to view\u003c/div\u003e\n            \u003c/div\u003e\n          \u003c/div\u003e\n          \u003cdiv class=\"notification_wrapper clearfix\"\u003e\n            \u003cdiv class=\"notification_icon red\"\u003e\u003c/div\u003e\n            \u003cdiv class=\"notification_text\"\u003e\n              \u003cdiv class=\"strong_text\"\u003e\u003cspan id=\"unanswered_question_count\"\u003e\u003c/span\u003e unanswered questions \u003cdiv class=\"qa_meta_timer_count qa_meta_timer_wrap\"\u003e(\u003cspan id=\"unanswered_question_count_on_timer\"\u003e\u003c/span\u003e\u003cdiv class=\"timer_delay_icon small green\"\u003e\u003c/div\u003e)\u003c/div\u003e\u003c/div\u003e\n              \u003cdiv class=\"light_text\" id=\"view_unanswered_questions\"\u003eclick to view\u003c/div\u003e\n            \u003c/div\u003e\n          \u003c/div\u003e\n          \u003cdiv class=\"notification_wrapper clearfix{{#if network.config.disable_followup_resolve}} invisible{{/if}}\"\u003e\n            \u003cdiv class=\"notification_icon red\"\u003e\u003c/div\u003e\n            \u003cdiv class=\"notification_text\"\u003e\n              \u003cdiv class=\"strong_text\"\u003e\u003cspan id=\"unresolved_followup_count\"\u003e\u003c/span\u003e unresolved followups\u003c/div\u003e\n              \u003cdiv class=\"light_text\" id=\"view_unanswered_questions\"\u003eclick to view\u003c/div\u003e\n            \u003c/div\u003e\n          \u003c/div\u003e\n          \u003cdiv class=\"notification_wrapper clearfix qa_meta_timer_wrap\"\u003e\n            \u003cdiv class=\"notification_icon red\"\u003e\u003c/div\u003e\n            \u003cdiv class=\"notification_text\"\u003e\n              \u003cdiv class=\"strong_text\"\u003e\u003cspan id=\"unanswered_question_count_on_timer_due\"\u003e\u003c/span\u003e posts due for an answer\u003c/div\u003e\n              \u003cdiv class=\"light_text\" id=\"view_unanswered_question_on_timer_due\"\u003eclick to view\u003c/div\u003e\n            \u003c/div\u003e\n          \u003c/div\u003e\n        \u003c/div\u003e\n\n        \u003cdiv class=\"homescreen_box_section\" style=\"width: 60%\"\u003e\n          \u003cdiv class=\"stat\"\u003e\u003cspan class=\"fixed_100\" \u003elicense status\u003c/span\u003e\u003cspan div id=\"license_status\"/\u003e\u003c/div\u003e\n          \u003cdiv class=\"stat\"\u003e\u003cspan class=\"fixed_100\" id=\"total_posts_count\"\u003e\u003c/span\u003etotal posts\u003c/div\u003e\n          \u003cdiv class=\"stat\"\u003e\u003cspan class=\"fixed_100\" id=\"total_contributions_count\"\u003e\u003c/span\u003etotal contributions\u003c/div\u003e\n          \u003cdiv class=\"stat\"\u003e\u003cspan class=\"fixed_100\" id=\"instructors_response_count\"\u003e\u003c/span\u003einstructors' responses\u003c/div\u003e\n          \u003cdiv class=\"stat\"\u003e\u003cspan class=\"fixed_100\" id=\"students_response_count\"\u003e\u003c/span\u003estudents' responses\u003c/div\u003e\n          \u003cdiv class=\"stat\"\u003e\u003cspan class=\"fixed_100\" id=\"average_response_time\"\u003e\u003c/span\u003eavg. response time\u003c/div\u003e\n          \u003cdiv id=\"warning-box\"\u003e\n            \u003ch6\u003e\n              \u003cspan style=\"display: none\" id=\"warning-box-one\"\u003eYou will need an active Piazza license in \u003cspan id=\"warning-box-one-when\"\u003e3 months\u003c/span\u003e.\u003c/span\u003e\n              \u003cspan style=\"display: none\" id=\"warning-box-two\"\u003eStarting August 1, 2021, premium features will no longer be available with the contribution-supported option.\u003c/span\u003e\n              \u003cspan style=\"display: none\" id=\"warning-box-three\"\u003eYour Piazza license is coming up for renewal at the end of \u003cspan id=\"warning-box-three-term\"\u003e\u003c/span\u003e.\u003c/span\u003e\n            \u003c/h6\u003e\n            Please email our team at \u003ca href=\"mailto:team@piazza.com\"\u003eteam@piazza.com\u003c/a\u003e to engage on a site license.\n            \u003ca href=\"https://support.piazza.com/support/solutions/articles/48001161300-paid-model-for-piazza-q-a\" target=\"_blank\"\u003eClick here\u003c/a\u003e to see details on our licensing model.\n          \u003c/div\u003e\n\n        \u003c/div\u003e\n      \u003c/div\u003e\n      \u003cdiv class=\"homescreen_progressbar_wrapper\"\u003e\n        \u003cdiv class=\"progressbar_header clearfix\"\u003e\n          \u003ch4\u003eStudent Enrollment\u003c/h4\u003e\n          \u003cdiv class=\"est_enrollment_wrapper view\"\u003e\n            ..out of \u003cspan class=\"current_est_enrollment\"\u003e\u003c/span\u003e (estimated) \u003ca id=\"editEstEnrollmentId\" href=\"#\" data-pats=\"edit_est_enrollment\" onclick=\"$('.est_enrollment_wrapper').hide();$('.est_enrollment_wrapper.edit').show();$('#new_est_enrollment').focus();return false;\"\u003eEdit\u003c/a\u003e\n          \u003c/div\u003e\n          \u003cdiv class=\"est_enrollment_wrapper edit\" style=\"display:none;\"\u003e\n            Estimated class size: \u003cinput type=\"text\" id=\"new_est_enrollment\" class=\"new_est_enrollment\" value=\"\"/\u003e \u003ca href=\"#\" id=\"saveEnrollmentId\" data-pats=\"save_enrollment\" onclick=\"P.homescreen.saveEnrollment(); return false;\"\u003eOK\u003c/a\u003e\n          \u003c/div\u003e\n        \u003c/div\u003e\n        \u003cdiv class=\"progress progress-success homescreen_progressbar\"\u003e\n          \u003cdiv class=\"bar bar_students\" style=\"width: 0%; text-align:left;\"\u003e\n            \u003cspan class=\"progress_count\"\u003e\u003cspan class=\"progress_count_number_students\"\u003e7\u003c/span\u003e\u0026nbsp;enrolled\u003c/span\u003e\n          \u003c/div\u003e\n        \u003c/div\u003e\n      \u003c/div\u003e\n      \u003cdiv class=\"pull-right app_store_container\"\u003e\n        \u003cdiv class=\"download_on_app_store_popup_container clearfix\"\u003e\n          \u003ca href=\"https://play.google.com/store/apps/details?id=com.piazza.android\" target=\"_blank\" class=\"android_store_img pull-right\"\u003e\u003c/a\u003e\n          \u003ca href=\"https://itunes.apple.com/us/app/piazza/id453142230\" target=\"_blank\" class=\"iphone_store_img pull-right\"\u003e\u003c/a\u003e\n          \u003cspan class=\"download_text pull-right\"\u003eDownload us in the app store:\u003c/span\u003e\n        \u003c/div\u003e\n      \u003c/div\u003e\n    \u003c/div\u003e\n\n      \u003c!-- Careers notifications by Sunthar --\u003e\n      \u003cdiv id=\"the_caag_wrapper\" style=\"display: none;\"\u003e\n        \u003cdiv class=\"homescreen_section_header\" style=\"text-align: left\" id=\"the_caag_title\"\u003e\n          Network at a Glance\n        \u003c/div\u003e\n        \u003cdiv class=\"homescreen_box clearfix careers_notification_homescreen_box\" id=\"the_caag\"\u003e\n          \u003cdiv class=\"not_opted_in_container clearfix\" style=\"display:none;\"\u003e\n            \u003cdiv class=\"not_opted_in\"\u003e\n              \u003cdiv class=\"private_icon\"\u003e\u003c/div\u003e\n              \u003cdiv class=\"left_section pull-left\"\u003e\n                \u003cdiv class=\"first_line\"\u003eYour profile is not published. To allow companies to discover and contact you, publish your profile.\u003c/div\u003e\n                \u003ca class=\"helper\" data-toggle=\"modal\" data-target=\"#profile_not_published_modal\"\u003eWhat does this mean?\u003c/a\u003e\n              \u003c/div\u003e\n              \u003cdiv class=\"right_section pull-right\"\u003e\n                \u003cbutton data-toggle=\"modal\"  data-toggle=\"modal\" onclick=\"P.homescreen.optInCaag(true)\" id=\"publish_profile_button\" class=\"btn btn-sm btn-success\"\u003ePublish Profile\u003c/button\u003e\n              \u003c/div\u003e\n            \u003c/div\u003e\n          \u003c/div\u003e\n          \u003cdiv class=\"clearfix caag_box\"\u003e\n            \u003cdiv class=\"homescreen_box_section\" id=\"added_companies_section\"\u003e\n              \u003cdiv class=\"icon_section\"\u003e\n                \u003ca class=\"notification_wrapper caag_notification clearfix\" href=\"/careers/dashboard/#my_profile/network\" target='_blank'\u003e\n                  \u003cdiv class=\"notification_icon red\" id=\"connections_count_icon\"\u003e\u003c/div\u003e\n                  \u003cdiv class=\"notification_text\"\u003e\n                    \u003cdiv class=\"strong_text\"\u003e\u003cspan id=\"total_connections_count\"\u003e\u003c/span\u003e total connections\u003c/div\u003e\n                    \u003cdiv class=\"light_text\" id=\"view_total_connections\"\u003eclick to view\u003c/div\u003e\n                  \u003c/div\u003e\n                \u003c/a\u003e\n                \u003ca class=\"notification_wrapper caag_notification clearfix\" id=\"profile_completeness_section\" href=\"/careers/dashboard/#my_profile\" target='_blank'\u003e\n                  \u003cdiv class=\"notification_icon red\" id=\"profile_completed_icon\"\u003e\u003c/div\u003e\n                  \u003cdiv class=\"notification_text\"\u003e\n                    \u003cdiv class=\"strong_text\"\u003e\u003cspan id=\"percent_profile_completed\"\u003e\u003c/span\u003e profile completed\u003c/div\u003e\n                    \u003cdiv class=\"light_text\" id=\"view_profile\"\u003eclick to view\u003c/div\u003e\n                  \u003c/div\u003e\n                \u003c/a\u003e\n                \u003ca class=\"notification_wrapper caag_notification clearfix\" href=\"/careers/dashboard/#companies\" target='_blank'\u003e\n                  \u003cdiv class=\"notification_icon red\" id=\"companies_viewed_icon\"\u003e\u003c/div\u003e\n                  \u003cdiv class=\"notification_text\"\u003e\n                    \u003cdiv class=\"strong_text\"\u003e\u003cspan id=\"num_companies_viewed\"\u003e\u003c/span\u003e\u003c/div\u003e\n                    \u003cdiv class=\"light_text\" id=\"view_companies_viewed\"\u003eclick to view\u003c/div\u003e\n                  \u003c/div\u003e\n                \u003c/a\u003e\n              \u003c/div\u003e\n              \u003cdiv class=\"companies_recently_added_wrapper\"\u003e\n                \u003ch4 class=\"companies_recently_added_header\"\u003eCompanies recently added\u003c/h4\u003e\n                \u003cul class=\"companies_recently_added\"\u003e\n                  \u003cli\u003e\n                    \u003ca id=\"company_link_0\" href=\"/careers/dashboard\" target='_blank'\u003e\u003cimg id=\"comp_img_0\" src=\"\" alt=\"\"\u003e\u003c/a\u003e\n                  \u003c/li\u003e\n                  \u003cli\u003e\n                    \u003ca id=\"company_link_1\" href=\"/careers/dashboard\" target='_blank'\u003e\u003cimg id=\"comp_img_1\" src=\"\" alt=\"\"\u003e\u003c/a\u003e\n                  \u003c/li\u003e\n                  \u003cli\u003e\n                    \u003ca id=\"company_link_2\" href=\"/careers/dashboard\" target='_blank'\u003e\u003cimg id=\"comp_img_2\" src=\"\" alt=\"\"\u003e\u003c/a\u003e\n                  \u003c/li\u003e\n                  \u003cli\u003e\n                    \u003ca id=\"company_link_3\" href=\"/careers/dashboard\" target='_blank'\u003e\u003cimg id=\"comp_img_3\" src=\"\" alt=\"\"\u003e\u003c/a\u003e\n                  \u003c/li\u003e\n                  \u003cli\u003e\n                    \u003ca id=\"company_link_4\" href=\"/careers/dashboard\" target='_blank'\u003e\u003cimg id=\"comp_img_4\" src=\"\" alt=\"\"\u003e\u003c/a\u003e\n                  \u003c/li\u003e\n                \u003c/ul\u003e\n              \u003c/div\u003e\n            \u003c/div\u003e\n            \u003cdiv class=\"homescreen_box_section\" id=\"job_status_section\"\u003e\n              \u003cdiv id=\"job_status_section_div\"\u003e\n                \u003clabel class=\"job_label\" for=\"job_status\"\u003e\u003cspan id=\"job_status_year_homescreen\"\u003e\u003c/span\u003e Job Status: \u003c/label\u003e\n                \u003cselect class=\"job_status_select\" name=\"position_status\" id=\"job_status\"\u003e\n                  \u003coption selected disabled value=\"none\"\u003e\u003c/option\u003e\n                  \u003coption value=\"seeking_full\"\u003eSeeking Full Time\u003c/option\u003e\n                  \u003coption value=\"seeking_intern\"\u003eSeeking Internship\u003c/option\u003e\n                  \u003coption value=\"accepted\"\u003eAccepted Position\u003c/option\u003e\n                \u003c/select\u003e\n              \u003c/div\u003e\n              \u003cdiv class=\"confirmation_wrapper clearfix\" id=\"headed_to_confirmation\" style=\"display: none;\"\u003e\n            \u003ch5 class=\"confirmation_text\"\u003eHeaded to: \u003cspan class=\"headed_to_company_name\" id=\"headed_to_company\"\u003e\u003c/span\u003e \u003ca href=\"#\" onclick=\"P.homescreen.reshowCaagJobStatus()\"\u003eedit\u003c/a\u003e\u003c/h5\u003e\n          \u003c/div\u003e\n              \u003cdiv id=\"headed_to_fields\"\u003e\n                \u003clabel class=\"headed_to\" for=\"headed_to\"\u003eHeaded to\u003c/label\u003e\n                \u003clabel class=\"job_label\" for=\"company_caag\"\u003eCompany: \u003c/label\u003e\u003cinput class=\"job_input\" type=\"text\" id=\"company_caag\"\u003e\n                \u003clabel class=\"job_label\" for=\"role_caag\"\u003eRole: \u003c/label\u003e\u003cinput class=\"job_input\" type=\"text\" id=\"role_caag\"\u003e\n                \u003clabel class=\"job_label\" for=\"location_caag\"\u003eLocation: \u003c/label\u003e\u003cinput class=\"job_input\" type=\"text\" id=\"location_caag\"\u003e\n                \u003cbutton class=\"btn btn-success btn-mini update_job_status_caag\" id=\"update_caag\"\u003eUpdate\u003c/button\u003e\n              \u003c/div\u003e\n              \u003cdiv class=\"confirmation_wrapper clearfix\" id=\"worked_at_confirmation\"\u003e\n            \u003ch5 class=\"confirmation_text\"\u003eWorked at: \u003cspan style=\"display: none;\" class=\"not_set\" id=\"work_not_set\"\u003eNot set\u003c/span\u003e\u003cspan class=\"headed_to_company_name\" id=\"worked_at_company\"\u003e\u003c/span\u003e \u003ca href=\"#\" onclick=\"P.homescreen.editWorkedAt()\"\u003eedit\u003c/a\u003e\u003c/h5\u003e\n          \u003c/div\u003e\n          \u003cdiv id=\"worked_at_list\" style=\"display: none;\" class=\"clearfix\"\u003e\n            \u003clabel class=\"headed_to\" for=\"headed_to\"\u003eWorked at:\u003c/label\u003e\n            {{#each workedAt}}\n            \u003cdiv class=\"worked_at_field_wrapper clearfix\"\u003e\n              \u003cdiv id=\"worked_at_fields\"\u003e\n                \u003clabel class=\"job_label\" for=\"company_caag\"\u003eCompany: \u003c/label\u003e\u003cinput class=\"job_input\" type=\"text\" id=\"company_caag_{{@index}}\"\u003e\n                \u003clabel class=\"job_label\" for=\"role_caag\"\u003eRole: \u003c/label\u003e\u003cinput class=\"job_input\" type=\"text\" id=\"role_caag_{{@index}}\"\u003e\n                \u003clabel class=\"job_label\" for=\"location_caag\"\u003eLocation: \u003c/label\u003e\u003cinput class=\"job_input\" type=\"text\" id=\"location_caag_{{@index}}\"\u003e\n                \u003clabel class=\"job_label\" for=\"year_caag\"\u003eYear: \u003c/label\u003e\u003cselect class=\"job_input\" type=\"text\" id=\"year_caag_{{@index}}\"\u003e\n                  \u003coption value=\"2015\"\u003e2015\u003c/option\u003e\n                  \u003coption value=\"2014\"\u003e2014\u003c/option\u003e\n                  \u003coption value=\"2013\"\u003e2013\u003c/option\u003e\n                  \u003coption value=\"2012\"\u003e2012\u003c/option\u003e\n                  \u003coption value=\"2011\"\u003e2011\u003c/option\u003e\n                \u003c/select\u003e\n              \u003c/div\u003e\n\n            \u003c/div\u003e\n              {{/each}}\n            \u003cdiv class=\"clearfix\"\u003e\n                \u003cbutton class=\"btn btn-success btn-mini update_job_status_caag\" id=\"update_worked_at\" onclick=\"P.homescreen.saveWorkedAt()\"\u003eUpdate\u003c/button\u003e\n                \u003ca href=\"#\" class=\"add_another_company\" onclick=\"P.homescreen.addNewWorkedAt()\"\u003eadd another company\u003c/a\u003e\n              \u003c/div\u003e\n          \u003c/div\u003e\n        \u003cdiv class=\"confirmation_wrapper clearfix\"\u003e\n          \u003ch5 class=\"confirmation_text\"\u003eVisa requirement: \u003cspan class=\"not_set\" id=\"visa_not_set\"\u003eNot set\u003c/span\u003e\u003cspan class=\"headed_to_company_name\" style=\"display: none;\" id=\"visa_set\"\u003e\u003c/span\u003e \u003ca href=\"#select_visa_modal\" role=\"button\" data-toggle=\"modal\"\u003eedit\u003c/a\u003e\u003c/h5\u003e\n        \u003c/div\u003e\n            \u003c/div\u003e\n            \u003c!-- \u003cdiv class=\"homescreen_progressbar_wrapper\"\u003e --\u003e\n              \u003c!--\n              \u003cdiv class=\"progressbar_header clearfix\"\u003e\n                \u003ch4 class=\"company_searches_text\"\u003eCompany searches you're showing up in\u003c/h4\u003e\n              \u003c/div\u003e\n              \u003cdiv class=\"progress progress-success homescreen_progressbar\"\u003e\n                \u003cdiv class=\"bar bar_students\" style=\"width: 0%; text-align:left;\" id=\"caag_bar\"\u003e\n                  \u003cspan class=\"progress_count\"\u003e\u003cspan class=\"progress_count_number_students\" id=\"search_level\"\u003e\u003c/span\u003e\u0026nbsp;\u003c/span\u003e\n                \u003c/div\u003e\n              \u003c/div\u003e\n            --\u003e\n              \u003c!-- \u003cdiv id=\"search_list\"\u003eCompanies that searched for you include: \u003cspan class=\"search_list_companies\"\u003e\u003c/span\u003e\u003c/div\u003e --\u003e\n            \u003c!-- \u003c/div\u003e --\u003e\n          \u003c/div\u003e\n\n\n          \u003cdiv class=\"clearfix\"\u003e\n            \u003cul class=\"careers_notifications_wrapper caag_nof_wrapper\"\u003e\n            \u003c!-- Note: Uncomment this pinned element to add currently applicable info and special announcements to the CAAG,\n    for example, when announcing TT winners. --\u003e\n              \u003c!--\u003cli\u003e\n                \u003ca class=\"clearfix\" id=\"nof_box_pinned\" style=\"display: none;\"\u003e\n                  \u003cspan class=\"careers_notification_icon pinned\" id=\"nof_icon_pinned\"\u003e\u003c/span\u003e\n                  \u003cspan class=\"careers_notification_text\"\u003e\n                    \u003cspan id=\"nof_text_pinned\"\u003e\u003cb\u003eCongratulations to our Silicon Valley Tech Tour finalists:\u003c/b\u003e Eduardo Ramirez, Scott Hesterberg, Elise Green, Andreas Ricci, Allie Ivener, Joshua Meier, Eric Brownrout, Philip Bale, Jonathan Dubin, Thao Bach, Awet Alazar, Keenan Molner, Emily Meigs, Michelle Green, Naji Dmeiri, and Kelsey Hrubes\u003c/span\u003e\n                  \u003c/span\u003e\n                \u003c/a\u003e\n              \u003c/li\u003e--\u003e\n\n              \u003c!-- \u003cli\u003e\n                \u003ca href=\"\" class=\"clearfix\" id=\"nof_box_0\" style=\"display:none\"\u003e\n                  \u003cspan class=\"careers_notification_icon search\" id=\"nof_icon_0\"\u003e\u003c/span\u003e\n                  \u003cspan class=\"careers_notification_text\"\u003e\n                    \u003cspan id=\"nof_text_0\"\u003eAirbnb and 25 other companies ran searches that you showed up in. Complete your profile to rank higher in search results.\u003c/span\u003e\n                  \u003c/span\u003e\n                \u003c/a\u003e\n              \u003c/li\u003e\n              \u003cli\u003e\n                \u003ca href=\"\" class=\"clearfix\" id=\"nof_box_1\" style=\"display:none\"\u003e\n                  \u003cspan class=\"careers_notification_icon event\" id=\"nof_icon_1\"\u003e\u003c/span\u003e\n                  \u003cspan class=\"careers_notification_text\"\u003e\n                    \u003cspan id=\"nof_text_1\"\u003ePinterest and 2 other companies published events on Piazza Careers. Visit the events dashboard.\u003c/span\u003e\n                  \u003c/span\u003e\n                \u003c/a\u003e\n              \u003c/li\u003e\n              \u003cli\u003e\n                \u003ca href=\"\" class=\"clearfix\" id=\"nof_box_2\" style=\"display:none\"\u003e\n                  \u003cspan class=\"careers_notification_icon profile\" id=\"nof_icon_2\"\u003e\u003c/span\u003e\n                  \u003cspan class=\"careers_notification_text\"\u003e\n                    \u003cspan id=\"nof_text_2\"\u003e22 of your classmates viewed employee experiences on OneLogin’s company profile. View OneLogin.\u003c/span\u003e\n                  \u003c/span\u003e\n                \u003c/a\u003e\n              \u003c/li\u003e --\u003e\n\n\n              \u003c!-- NOTE: this code is the old caag code with students answers to questions \u003cli\u003e\n                \u003ca href=\"\" class=\"clearfix\" id=\"nof_box_0\" style=\"display:none\"\u003e\n                  \u003cspan class=\"careers_notification_icon search\" id=\"nof_icon_0\"\u003e\u003c/span\u003e\n                  \u003cspan class=\"careers_notification_text\"\u003e\n                    Read \u003cspan class=\"nof_name\" id=\"nof_exp_name_0\"\u003e\u003c/span\u003e's answer to \"\u003cspan class=\"nof_question\" id=\"nof_exp_question_0\"\u003e\u003c/span\u003e\" on \u003cspan class=\"nof_company\" id=\"nof_exp_company_0\"\u003e\u003c/span\u003e's company profile: \u003cspan class=\"nof_answer\" id=\"nof_exp_answer_0\"\u003e\u003c/span\u003e\n                  \u003c/span\u003e\n                \u003c/a\u003e\n              \u003c/li\u003e\n              \u003cli\u003e\n                \u003ca href=\"\" class=\"clearfix\" id=\"nof_box_1\" style=\"display:none\"\u003e\n                  \u003cspan class=\"careers_notification_icon event\" id=\"nof_icon_1\"\u003e\u003c/span\u003e\n                  \u003cspan class=\"careers_notification_text\"\u003e\n                    Read \u003cspan class=\"nof_name\" id=\"nof_exp_name_1\"\u003e\u003c/span\u003e's answer to \"\u003cspan class=\"nof_question\" id=\"nof_exp_question_1\"\u003e\u003c/span\u003e\" on \u003cspan class=\"nof_company\" id=\"nof_exp_company_1\"\u003e\u003c/span\u003e's company profile: \u003cspan class=\"nof_answer\" id=\"nof_exp_answer_1\"\u003e\u003c/span\u003e\n                  \u003c/span\u003e\n                \u003c/a\u003e\n              \u003c/li\u003e\n              \u003cli\u003e\n                \u003ca href=\"\" class=\"clearfix\" id=\"nof_box_2\" style=\"display:none\"\u003e\n                  \u003cspan class=\"careers_notification_icon profile\" id=\"nof_icon_2\"\u003e\u003c/span\u003e\n                  \u003cspan class=\"careers_notification_text\"\u003e\n                    Read \u003cspan class=\"nof_name\" id=\"nof_exp_name_2\"\u003e\u003c/span\u003e's answer to \"\u003cspan class=\"nof_question\" id=\"nof_exp_question_2\"\u003e\u003c/span\u003e\" on \u003cspan class=\"nof_company\" id=\"nof_exp_company_2\"\u003e\u003c/span\u003e's company profile: \u003cspan class=\"nof_answer\" id=\"nof_exp_answer_2\"\u003e\u003c/span\u003e\n                  \u003c/span\u003e\n                \u003c/a\u003e\n              \u003c/li\u003e\n            \u003c/ul\u003e\n\n            \u003c!-- \u003cdiv class=\"discoverability_wrapper\"\u003e\n              \u003ca href=\"#\" id=\"discoverability_link_wrapper\" target=\"_blank\"\u003e\n                \u003cdiv class=\"discoverability_meter percent_20\" id=\"nof_thermometer\"\u003e\n                  \u003cspan class=\"glass\"\u003e\n                      \u003cspan class=\"amount\"\u003e\u003c/span\u003e\n                  \u003c/span\u003e\n                  \u003cdiv class=\"bulb\"\u003e\n                      \u003cspan class=\"green-circle\" id=\"nof_discoverability\"\u003e\n                        20%\n                      \u003c/span\u003e\n                      \u003cspan class=\"filler\"\u003e\n                          \u003cspan\u003e\u003c/span\u003e\n                      \u003c/span\u003e\n                  \u003c/div\u003e\n                \u003c/div\u003e\n                \u003cdiv class=\"discoverability_label\"\u003e\n                  Your Profile Discoverability\n                \u003c/div\u003e\n              \u003c/a\u003e\n            \u003c/div\u003e --\u003e\n\n          \u003c/div\u003e\n\n        \u003c/div\u003e\n        \u003cdiv id=\"caag_disclaimer\"\u003e\n          *Companies will never, ever see your questions or answers on Piazza, and we do not expose any personal information unless you allow it.\n        \u003c/div\u003e\n      \u003c/div\u003e\n\n\n\n    {{#unless user.isTA}}\n\n\n\n\n\n\n\n\n    \u003cdiv class=\"homescreen_box clearfix share_your_class_box\"\u003e\n      \u003cdiv class=\"clearfix\"\u003e\n        \u003ch1\u003eShare Your Class\u003c/h1\u003e\n        \u003cp style=\"margin-bottom: 10px;\"\u003eProfessors appreciate Piazza best when they see how it is being used.\u003c/p\u003e\n        \u003cp style=\"margin-bottom: 10px;\"\u003eAllow colleagues to view your class through a demo link - a restricted, read only version of your class where all students' names are anonymized and all student information hidden.\u003c/p\u003e\n        \u003ctextarea data-pats=\"demo_link\" class=\"demo_link\" onclick=\"this.focus();this.select();\" readonly=\"readonly\"\u003ehttps://{{info.myhost}}/demo_login?nid={{network.id}}\u0026auth={{network.auth}}\u003c/textarea\u003e\n        \u003cdiv style='font-size:11px;text-align:center;color:#888;'\u003eOpening this link in the same browser will log you out as {{user.email}}\u003c/div\u003e\n      \u003c/div\u003e\n    \u003c/div\u003e\n\n    \u003cdiv class=\"homescreen_box clearfix\" id=\"instructor_news\" style=\"display:none;\"\u003e\n      \u003cdiv class=\"homescreen_close_button close_prof_news\" style=\"visibility: visible;\"\u003e\n        \u003ci class=\"icon-remove\"\u003e\u003c/i\u003e\n      \u003c/div\u003e\n      \u003cdiv class=\"prof_message clearfix\" id=\"instructor_news_container\"\u003e\n      \u003c/div\u003e\n    \u003c/div\u003e\n\n    \u003cdiv class=\"homescreen_box clearfix homescreen_careers_is_here\" style=\"display:none;\"\u003e\n      \u003cdiv class=\"homescreen_close_button close_prof_careers_warning\" style=\"visibility: visible;\"\u003e\n        \u003ci class=\"icon-remove\"\u003e\u003c/i\u003e\n      \u003c/div\u003e\n      \u003cdiv class=\"prof_message clearfix\"\u003e\n        \u003cp\u003eDear Professors:\u003c/p\u003e\n        \u003cp\u003ePiazza was built on the founding principle of creating inclusion in the classroom. I was one of 3 women studying computer science, and too shy to ask for help from my male classmates, I started Piazza so every student could have a voice.\u003c/p\u003e\n\n        \u003cp\u003eWe recently compiled a Toolkit for Professors. This toolkit includes an in-depth product guide, professor case studies, tips for a successful class, boosting minorities’ participation in the class, information on FERPA compliance, student privacy and on our optional careers-oriented product offering Piazza Network that serves to aid students in getting jobs and internships.\u003c/p\u003e\n\n        \u003cp\u003eWe hope this serves as a helpful resource to you and other faculty members in your department as you adopt Piazza to further your students’ learning and outcomes: \u003ca href=\"https://piazza.com/toolkit\" target=\"_blank\"\u003epiazza.com/toolkit\u003c/a\u003e\u003c/p\u003e\n        \u003cul\u003e\n          \u003cli\u003e\n            \u003ca href=\"/pdfs/piazza_product_introduction.pdf\" target=\"_blank\"\u003eProduct PDF\u003c/a\u003e\u003cbr /\u003e\n          \u003c/li\u003e\n          \u003cli\u003e\n            \u003ca href=\"/professors\" target=\"_blank\"\u003eProfessor Success Stories\u003c/a\u003e\u003cbr /\u003e\n          \u003c/li\u003e\n          \u003cli\u003e\n            \u003ca href=\"/about/story\" target=\"_blank\"\u003eOur Story\u003c/a\u003e\u003cbr /\u003e\n          \u003c/li\u003e\n          \u003cli\u003e\n            \u003ca href=\"/network\" target=\"_blank\"\u003eThe Piazza Network\u003c/a\u003e\u003cbr /\u003e\n          \u003c/li\u003e\n          \u003cli\u003e\n            \u003ca href=\"/boostingparticipation\" target=\"_blank\"\u003eBoosting Women's and Minorities' Participation\u003c/a\u003e\u003cbr /\u003e\n          \u003c/li\u003e\n          \u003cli\u003e\n            \u003ca href=\"/demopiazza\" target=\"_blank\"\u003eDemo Piazza to Other Faculty\u003c/a\u003e\u003cbr /\u003e\n          \u003c/li\u003e\n          \u003cli\u003e\n            \u003ca href=\"/legal/ferpa\" target=\"_blank\"\u003eFERPA Compliance\u003c/a\u003e\u003cbr /\u003e\n          \u003c/li\u003e\n        \u003c/ul\u003e\n\n        \u003cp\u003eThank you for your support, today and always. Should you have any questions or feedback, please don’t hesitate to email me at \u003ca href=\"mailto:pooja@piazza.com\" target=\"_blank\"\u003epooja@piazza.com\u003c/a\u003e.\u003c/p\u003e\n\n\n        \u003cp class=\"no_bottom_margin\"\u003eWarm regards,\u003c/p\u003e\n        \u003cp class=\"no_bottom_margin\"\u003ePooja\u003c/p\u003e\n        \u003cp\u003eFounder \u0026amp; CEO, Piazza\u003c/p\u003e\n        \u003cp\u003eP.S. You might be interested in \u003ca href=\"https://www.tandfonline.com/doi/abs/10.1080/15391523.2017.1343692?journalCode=ujrt20\" target=\"_blank\"\u003ethis paper\u003c/a\u003e in the Journal of Research on Technology in Education that analyzed how Piazza enables student interactivity and the formation of a knowledge community.\u003c/p\u003e\n        \u003cp class=\"muted\"\u003eYou own the content you post on Piazza. \u003ca href=\"/terms\" target=\"_blank\"\u003eLearn more\u003c/a\u003e about our Terms of Service. \u003ca href=\"/ferpa\" target=\"_blank\"\u003eLearn more\u003c/a\u003e about how Piazza complies with FERPA.\n\u003c/p\u003e\n      \u003c/div\u003e\n    \u003c/div\u003e\n    {{/unless}}\n  \u003c/div\u003e\n\n\n  \u003cdiv class=\"ia2a\"\u003e\n    \u003cdiv id=\"tips_tricks\" class=\"homescreen_box ia2a clearfix\"\u003e\n      \u003cdiv class=\"homescreen_close_button\"\u003e\u003ci class=\"icon-remove\"\u003e\u003c/i\u003e\u003c/div\u003e\n      \u003ch3\u003eRead tips and tricks for a successful Piazza Class\u003c/h3\u003e\n      \u003cdiv class=\"homescreen_content clearfix\"\u003e\n        \u003cdiv class=\"tips_n_tricks\"\u003e\n          \u003cspan class=\"private_icon\"\u003ePrivate\u003c/span\u003e\n          \u003cspan class=\"title\"\u003eTips \u0026amp; Tricks for a successful class\u003c/span\u003e\n          \u003cdiv class=\"note_icon\"\u003e\u003c/div\u003e\n        \u003c/div\u003e\n      \u003c/div\u003e\n      \u003cdiv id=\"read_tips_tricks\" class=\"homescreen_action\"\u003eread now\u003c/div\u003e\n    \u003c/div\u003e\n    \u003cdiv id=\"first_day\" class=\"homescreen_box ia2a clearfix resolved\"\u003e\n      \u003cdiv class=\"homescreen_close_button\"\u003e\u003ci class=\"icon-remove\"\u003e\u003c/i\u003e\u003c/div\u003e\n      \u003ch3 class='first_view'\u003eWhen is the first day of class?\u003c/h3\u003e\n      \u003ch3 class='edit_view' style=\"display: none;\"\u003eNeed to edit your class start date?\u003c/h3\u003e\n      \u003ch3 class='second_view' style=\"display: none;\"\u003eThank you for entering your class start date!\u003c/h3\u003e\n      \u003cdiv class=\"homescreen_content clearfix\"\u003e\n        \u003cdiv class=\"first_view edit_view\"\u003e\n            \u003cdiv class=\"start_date_input\"\u003e\n              \u003cinput type=\"text\" name=\"first_day_of_class\" id=\"first_day_of_class\" data-date-format=\"mm/dd/yyyy\"/\u003e\n              \u003cbutton class=\"btn btn-small\" id=\"save_first_day\"\u003esave\u003c/button\u003e\n            \u003c/div\u003e\n        \u003c/div\u003e\n        \u003cdiv class=\"second_view\" style=\"display: none;\"\u003e\n          \u003cp\u003eYou (or another instructor) have already entered the class start date! To update the class start date, head to your \u003cstrong\u003eManage Class\u003c/strong\u003e tab.\u003c/p\u003e\n        \u003c/div\u003e\n      \u003c/div\u003e\n    \u003c/div\u003e\n    \u003cdiv id=\"student_progress_bar\" class=\"homescreen_box ia2a clearfix\"\u003e\n      \u003cdiv class=\"homescreen_content clearfix\"\u003e\n        \u003cdiv class=\"homescreen_progressbar_wrapper\"\u003e\n          \u003cdiv class=\"progressbar_header clearfix\"\u003e\n            \u003ch4\u003eStudent Enrollment\u003c/h4\u003e\n            \u003cdiv class=\"est_enrollment_wrapper view\"\u003e\n              ..out of \u003cspan class=\"current_est_enrollment\"\u003e\u003c/span\u003e (estimated) \u003ca href=\"#\" onclick=\"$('.est_enrollment_wrapper').hide();$('.est_enrollment_wrapper.edit').show();$('#new_est_enrollment').focus();return false;\"\u003eEdit\u003c/a\u003e\n            \u003c/div\u003e\n            \u003cdiv class=\"est_enrollment_wrapper edit\" style=\"display:none;\"\u003e\n              Estimated class size: \u003cinput type=\"text\" id=\"new_est_enrollment\" class=\"new_est_enrollment\" value=\"\"/\u003e \u003ca href=\"#\" onclick=\"P.homescreen.saveEnrollment(); return false;\"\u003eOK\u003c/a\u003e\n            \u003c/div\u003e\n          \u003c/div\u003e\n          \u003cdiv class=\"progress progress-success homescreen_progressbar\"\u003e\n            \u003cdiv class=\"bar bar_students\" style=\"width: 0%; text-align:left;\"\u003e\n              \u003cspan class=\"progress_count\"\u003e\u003cspan class=\"progress_count_number_students\"\u003e7\u003c/span\u003e\u0026nbsp;enrolled\u003c/span\u003e\n            \u003c/div\u003e\n          \u003c/div\u003e\n        \u003c/div\u003e\n      \u003c/div\u003e\n    \u003c/div\u003e\n    \u003cdiv id=\"enroll_students\" class=\"homescreen_box ia2a clearfix\"\u003e\n      \u003cdiv class=\"homescreen_close_button\"\u003e\u003ci class=\"icon-remove\"\u003e\u003c/i\u003e\u003c/div\u003e\n      \u003ch3 class=\"first_view\"\u003eEnroll your students\u003c/h3\u003e\n      \u003ch3 style=\"display: none;\" class=\"second_view\"\u003eThank you for enrolling your students!\u003c/h3\u003e\n      \u003cdiv class=\"homescreen_content clearfix\"\u003e\n        \u003cdiv class=\"first_view\"\u003e\n            \u003cp\u003ePaste email addresses below in any format. Or visit Manage Class page to upload your student roster or share your Class Signup Link.\u003c/p\u003e\n            \u003cp class=\"grey_text\"\u003e↳ Each will receive a welcome email.\u003c/p\u003e\n            \u003ctextarea id=\"add_students\" placeholder=\"john@email.com, smith@email.com\" class=\"enroll_members\"\u003e\u003c/textarea\u003e\n            \u003cbutton class=\"btn btn-primary invite_button\" id=\"save_enroll_students\"\u003eEnroll Students\u003c/button\u003e\n        \u003c/div\u003e\n        \u003cdiv class=\"second_view\" style=\"display: none;\"\u003e\n          \u003cp\u003eYou (or another instructor) have already enrolled students! To manage student enrollments, head to your \u003cstrong\u003eManage Class\u003c/strong\u003e tab.\u003c/p\u003e\n        \u003c/div\u003e\n        \u003cdiv class=\"progressbar_header clearfix\"\u003e\n          \u003ch4\u003eStudent Enrollment\u003c/h4\u003e\n          \u003cdiv class=\"est_enrollment_wrapper view\"\u003e\n            ..out of \u003cspan class=\"current_est_enrollment\"\u003e\u003c/span\u003e (estimated) \u003ca href=\"#\" onclick=\"$('.est_enrollment_wrapper').hide();$('.est_enrollment_wrapper.edit').show();$('#new_est_enrollment2').focus();return false;\"\u003eEdit\u003c/a\u003e\n          \u003c/div\u003e\n          \u003cdiv class=\"est_enrollment_wrapper edit\" style=\"display:none;\"\u003e\n            Estimated class size: \u003cinput type=\"text\" id=\"new_est_enrollment2\" value=\"\" class=\"new_est_enrollment\"/\u003e \u003ca href=\"#\" onclick=\"P.homescreen.saveEnrollment(); return false;\"\u003eOK\u003c/a\u003e\n          \u003c/div\u003e\n        \u003c/div\u003e\n        \u003cdiv class=\"progress progress-success homescreen_progressbar\"\u003e\n          \u003cdiv class=\"bar bar_students\" style=\"width: 0%; text-align:left;\"\u003e\n            \u003cspan class=\"progress_count\"\u003e\u003cspan class=\"progress_count_number_students\"\u003e7\u003c/span\u003e\u0026nbsp;enrolled\u003c/span\u003e\n          \u003c/div\u003e\n        \u003c/div\u003e\n      \u003c/div\u003e\n    \u003c/div\u003e\n    \u003cdiv id=\"access_code\" class=\"homescreen_box ia2a clearfix\"\u003e\n      \u003cdiv class=\"homescreen_close_button\"\u003e\u003ci class=\"icon-remove\"\u003e\u003c/i\u003e\u003c/div\u003e\n      \u003ch3 class='first_view'\u003eSet your class access code\u003c/h3\u003e\n      \u003ch3 class='edit_view' style=\"display: none;\"\u003eNeed to edit your class access code?\u003c/h3\u003e\n      \u003ch3 class='second_view' style=\"display: none;\"\u003eThank you for setting your access code!\u003c/h3\u003e\n      \u003cdiv class=\"homescreen_content clearfix\"\u003e\n        \u003cdiv class=\"first_view edit_view\"\u003e\n            \u003cp\u003eYour students will be required to enter this code in order to self-enroll in this class.\u003c/p\u003e\n            \u003cdiv class=\"start_date_input\"\u003e\n              \u003cinput type=\"text\" name=\"access_code_input\" id=\"access_code_input\" /\u003e\n              \u003cbutton class=\"btn btn-small\" id=\"save_access_code\"\u003esave\u003c/button\u003e\n            \u003c/div\u003e\n        \u003c/div\u003e\n        \u003cdiv class=\"second_view\" style=\"display: none;\"\u003e\n          \u003cp\u003eYou (or another instructor) have already confirmed a class access code. To update the access code, head to your \u003cstrong\u003eManage Class\u003c/strong\u003e tab.\u003c/p\u003e\n        \u003c/div\u003e\n      \u003c/div\u003e\n    \u003c/div\u003e\n\n    \u003cdiv id=\"enroll_inst_self\" class=\"homescreen_box ia2a clearfix\"\u003e\n      \u003cdiv class=\"homescreen_close_button\"\u003e\u003ci class=\"icon-remove\"\u003e\u003c/i\u003e\u003c/div\u003e\n      \u003ch3 class=\"ask_self\" style=\"display: block;\"\u003eAllow TAs/other instructors to enroll themselves?\u003c/h3\u003e\n      \u003cdiv class=\"homescreen_content clearfix\"\u003e\n        \u003cdiv class=\"first_view\"\u003e\n          \u003cdiv class=\"enroll_inst_self_actions\" style=\"text-align:center;\"\u003e\n            \u003cbutton class=\"btn btn-small\"\u003eyes\u003c/button\u003e\n            \u003cbutton class=\"btn btn-small\"\u003eno\u003c/button\u003e\n          \u003c/div\u003e\n        \u003c/div\u003e\n      \u003c/div\u003e\n    \u003c/div\u003e\n\n    \u003cdiv id=\"enroll_inst\" class=\"homescreen_box ia2a clearfix\"\u003e\n      \u003cdiv class=\"homescreen_close_button\"\u003e\u003ci class=\"icon-remove\"\u003e\u003c/i\u003e\u003c/div\u003e\n      \u003c!--\u003ch3 class=\"ask_view first_view\" style=\"display: block;\"\u003eAre there TAs/other instructors in your course?\u003c/h3\u003e--\u003e\n      \u003ch3 class=\"enroll_view first_view\" style=\"display: block;\"\u003eEnroll your fellow instructors\u003c/h3\u003e\n      \u003ch3 class=\"thanks_view second_view\" style=\"display: none;\"\u003eThank you for enrolling your instructors!\u003c/h3\u003e\n      \u003c!--\u003ch3 class=\"no_others_view\" style=\"display: none;\"\u003eNo other instructors\u003c/h3\u003e--\u003e\n      \u003cdiv class=\"homescreen_content clearfix\"\u003e\n        \u003c!--\u003cdiv class=\"first_view\"\u003e\n          \u003cdiv class=\"enroll_inst_actions\"\u003e\n            \u003cbutton class=\"btn btn-small btn-primary\"\u003eyes\u003c/button\u003e\n            \u003cbutton class=\"btn btn-small\"\u003eno\u003c/button\u003e\n          \u003c/div\u003e\n        \u003c/div\u003e--\u003e\n        \u003cdiv class=\"second_view enroll_view\" style=\"display: block;\"\u003e\n            \u003cp\u003ePaste email addresses below in any format. Or visit Manage Class page to share your Class Signup Link.\u003c/p\u003e\n            \u003cp class=\"grey_text\"\u003e↳ Once added, they receive a welcome email with a link to activate their Piazza account.\u003c/p\u003e\n            \u003ctextarea id=\"add_instructors\" placeholder=\"john@email.com, smith@email.com\" class=\"enroll_members\"\u003e\u003c/textarea\u003e\n            \u003cbutton class=\"btn btn-primary invite_button\" id=\"save_enroll_inst\"\u003eEnroll Instructors\u003c/button\u003e\n        \u003c/div\u003e\n        \u003cdiv class=\"third_view\" style=\"display: none;\"\u003e\n          \u003cp\u003eYou (or another instructor) have already enrolled instructors! To manage instructor enrollments, head to your \u003cstrong\u003eManage Class\u003c/strong\u003e tab.\u003c/p\u003e\n        \u003c/div\u003e\n        \u003cdiv class=\"no_others_view\" style=\"display: none;\"\u003e\n          \u003cp\u003eYou (or another instructor) have indicated that there are currently no other instructors in this class. To manage instructor enrollments, head to your \u003cstrong\u003eManage Class\u003c/strong\u003e tab.\u003c/p\u003e\n        \u003c/div\u003e\n      \u003c/div\u003e\n    \u003c/div\u003e\n\n\n    \u003cdiv id=\"setup_course_page\" class=\"homescreen_box ia2a clearfix\"\u003e\n      \u003cdiv class=\"homescreen_close_button\"\u003e\u003ci class=\"icon-remove\"\u003e\u003c/i\u003e\u003c/div\u003e\n      \u003ch3 class=\"first_view\" style=\"display: block;\"\u003eSet up your Course Page\u003c/h3\u003e\n      \u003ch3 class=\"second_view\" style=\"display: none;\"\u003eThank you for setting up your Course Page!\u003c/h3\u003e\n      \u003cdiv class=\"homescreen_content clearfix\"\u003e\n        \u003cdiv class=\"first_view\"\u003e\n          \u003cp\u003eAfter you \u003ca href=\"#\" onclick=\"PEM.fire('gotoCoursePage');return false;\"\u003eenter your course description\u003c/a\u003e,\n            make sure you \u003ca href=\"#\" onclick=\"PEM.fire('gotoCoursePage');return false;\"\u003eupload your syllabus\u003c/a\u003e\n            and \u003ca href=\"#\" onclick=\"PEM.fire('gotoCoursePage', 'staff');return false;\"\u003epost your office hours!\u003c/a\u003e\u003c/p\u003e\n        \u003c/div\u003e\n        \u003cdiv class=\"second_view\" style=\"display: none;\"\u003e\n          \u003cp\u003eYou (or another instructor) have already begun editing your Course Page! To continue to add and \u0026amp; revise content, click on the \u003cstrong\u003eCourse Page\u003c/strong\u003e button in the top bar.\u003c/p\u003e\n        \u003c/div\u003e\n      \u003c/div\u003e\n      \u003cdiv class=\"homescreen_action\" id=\"save_setup_course_page\"\u003edone\u003c/div\u003e\n    \u003c/div\u003e\n    \u003cdiv id=\"setup_sections\" class=\"homescreen_box ia2a clearfix\"\u003e\n      \u003cdiv class=\"homescreen_close_button\"\u003e\u003ci class=\"icon-remove\"\u003e\u003c/i\u003e\u003c/div\u003e\n      \u003ch3 class=\"first_view\" style=\"display: block;\"\u003eEnable Group Based Discussion\u003c/h3\u003e\n      \u003ch3 class=\"second_view\" style=\"display: none;\"\u003eThank you for setting up your Class Groups!\u003c/h3\u003e\n      \u003cdiv class=\"homescreen_content clearfix\"\u003e\n        \u003cdiv class=\"first_view\"\u003e\n          \u003cp\u003eThis feature allows students to post private messages to instructors and members of a predefined group, great for project teams or class groups.\u003c/p\u003e\n        \u003c/div\u003e\n        \u003cdiv class=\"second_view\" style=\"display: none;\"\u003e\n          \u003cp\u003eYou (or another instructor) have already begun creating groups for your class! To manage your class groups, head to your \u003cstrong\u003eManage Class\u003c/strong\u003e tab.\u003c/p\u003e\n        \u003c/div\u003e\n      \u003c/div\u003e\n      \u003cdiv class=\"homescreen_action\" id=\"save_setup_sections\"\u003elearn more\u003c/div\u003e\n    \u003c/div\u003e\n    \u003cdiv id=\"share_piazza\" class=\"homescreen_box ia2a clearfix\"\u003e\n      \u003cdiv class=\"homescreen_close_button\"\u003e\u003ci class=\"icon-remove\"\u003e\u003c/i\u003e\u003c/div\u003e\n      \u003ch3 class=\"first_view\" style=\"display: block\"\u003eShare Piazza with other professors\u003c/h3\u003e\n      \u003ch3 class=\"second_view\" style=\"display: none\"\u003eThank you for sharing Piazza with other professors!\u003c/h3\u003e\n      \u003cdiv class=\"homescreen_content clearfix\"\u003e\n        \u003cdiv class=\"first_view\"\u003e\n          \u003cp class=\"narrow_width\"\u003eClick the email icon to send a message to other professors about Piazza!\u003c/p\u003e\n          \u003cins class=\"share_icon email share_icon_email_button\"\u003e\u003c/ins\u003e\n        \u003c/div\u003e\n      \u003c/div\u003e\n    \u003c/div\u003e\n    \u003cdiv id=\"create_class\" class=\"homescreen_box ia2a clearfix\"\u003e\n      \u003cdiv class=\"homescreen_close_button\"\u003e\u003ci class=\"icon-remove\"\u003e\u003c/i\u003e\u003c/div\u003e\n      \u003ch3\u003eCreate another Piazza class\u003c/h3\u003e\n      \u003cdiv class=\"homescreen_content clearfix\"\u003e\n\n      \u003c/div\u003e\n      \u003cdiv class=\"homescreen_action\" id=\"create_a_new_class\"\u003eget started\u003c/div\u003e\n    \u003c/div\u003e\n  \u003c/div\u003e\n  \u003cdiv id=\"instructor_enroll_modal_wrapper\"\u003e\u003c/div\u003e\n  \u003cdiv id=\"share_email_modal\" class=\"modal hide fade\"\u003e\n    \u003cdiv class=\"modal-header\"\u003e\n      \u003cbutton type=\"button\" class=\"close\" data-dismiss=\"modal\" aria-hidden=\"true\"\u003e\u0026times;\u003c/button\u003e\n      \u003ch3\u003eEmail your friends about Piazza\u003c/h3\u003e\n    \u003c/div\u003e\n\n    \u003cdiv class=\"modal-body\"\u003e\n      \u003cdiv id=\"to-region\" class=\"content_section\"\u003e\n        \u003cp\u003eto:\u003c/p\u003e\n        \u003cinput type=\"text\" id=\"share_email_to\"\u003e\n      \u003c/div\u003e\n      \u003cdiv id=\"subject-region\" class=\"content_section\"\u003e\n        \u003cp\u003esubject:\u003c/p\u003e\n        \u003cinput type=\"text\" id=\"share_email_subject\" value=\"I think you might like Piazza!\"\u003e\n      \u003c/div\u003e\n      \u003cdiv id=\"content-region\" class=\"content_section\"\u003e\n        \u003cp\u003emessage:\u003c/p\u003e\n        \u003ctextarea id=\"share_email_content\"\u003eHello,\nI've been using Piazza this term to teach {{network.course_number}} and thought that you'd like it!\n\nPiazza is a free online gathering place where students can ask, answer, and explore under the guidance of their instructors. With Piazza, I can easily answer questions, manage course materials, and track student participation. Check it out and try it for yourself at https://piazza.com.\n\nWarm regards,\n{{user.name}}\u003c/textarea\u003e\n      \u003c/div\u003e\n    \u003c/div\u003e\n    \u003cdiv class=\"modal-footer\"\u003e\n        \u003ca class=\"btn cancel\" href=\"#\" onclick=\"return P.homescreen.sendShareEmail();\"\u003e\u003cspan\u003esend\u003c/span\u003e\u003c/a\u003e\n        \u003ca class=\"btn cancel\" href=\"#\" onclick=\"$('#share_email_modal').modal('hide');return false;\"\u003e\u003cspan\u003ecancel\u003c/span\u003e\u003c/a\u003e\n    \u003c/div\u003e\n  \u003c/div\u003e\n\n\u003c/div\u003e\n\u003cdiv class=\"modal hide fade terms_modal\" id=\"termsModal\" tabindex=\"-1\" role=\"dialog\" aria-hidden=\"true\"\u003e\n      \u003cdiv class=\"modal-header\"\u003e\n        \u003cbutton type=\"button\" class=\"close\" data-dismiss=\"modal\" aria-hidden=\"true\"\u003e×\u003c/button\u003e\n        \u003ch3 id=\"myModalLabel\"\u003eTerms of Service and Privacy Policy\u003c/