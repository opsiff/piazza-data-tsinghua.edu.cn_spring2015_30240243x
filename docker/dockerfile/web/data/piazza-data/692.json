34;w&#34;,stdout);\n while (scanf(&#34;va 0x%x, pa 0x%x\\n&#34;, &amp;va, &amp;pa) != EOF)\n {\n  pde_idx = (va &amp; 0xcffc0000) &gt;&gt; 22;\n  pde_ctx = ((pde_idx - 0x300 &#43; 1) &lt;&lt; 12) &#43; (0x3);\n  pte_idx = (va &amp; 0x003ff000) &gt;&gt; 12;\n  pte_ctx = (pa &amp; 0xfffff000) | (0x3);\n  printf(&#34;va 0x%08x, pa 0x%08x, &#34;, va, pa);\n  printf(&#34;pde_idx 0x%08x, &#34;, pde_idx);\n  printf(&#34;pde_ctx 0x%08x, &#34;, pde_ctx);\n  printf(&#34;pte_idx 0x%08x, &#34;, pte_idx);\n  printf(&#34;pte_ctx 0x%08x\\n&#34;, pte_ctx);\n }\n return 0;\n}</pre>\n<p></p>"}, {"folders": [], "updated": "2016-03-14T10:53:40Z", "uid": "i0d4mr9tga62ki", "created": "2016-03-14T10:53:40Z", "type": "feedback", "id": "ilrvgs7w2yitd", "anon": "no", "bucket_name": "Week 3/13 - 3/19", "config": {}, "bucket_order": 55, "data": null, "children": [], "subject": "<p>学号 : 2013011303 2013012213</p>\n<p><a href=\"https://github.com/honoumi/ucore_lab/blob/master/spoc/spoc_7.cpp\" target=\"_blank\">https://github.com/honoumi/ucore_lab/blob/master/spoc/spoc_7.cpp</a></p>"}, {"folders": [], "updated": "2016-03-14T11:41:24Z", "uid": "i0f7v0qgfm61s6", "created": "2016-03-14T11:41:24Z", "type": "feedback", "id": "ilrx65nd6j76i2", "anon": "no", "bucket_name": "Week 3/13 - 3/19", "config": {}, "bucket_order": 55, "data": null, "children": [], "subject": "<p></p>\n<p>徐涵 2013011355</p>\n<p>白家松 2013011339</p>\n<p>代码&#xff1a;<a href=\"https://github.com/dpatrickx/course-os-spoc-discussion/blob/master/spoc7/trans.cpp\" target=\"_blank\">code</a></p>\n<p>输入文件&#xff1a;<a href=\"https://github.com/dpatrickx/course-os-spoc-discussion/blob/master/spoc7/input\" target=\"_blank\">input</a></p>\n<p>结果文件&#xff1a;<a href=\"https://github.com/dpatrickx/course-os-spoc-discussion/blob/master/spoc7/output\" target=\"_blank\">output</a></p>"}, {"folders": [], "updated": "2016-03-14T11:53:34Z", "uid": "i28t9sj8bas79c", "created": "2016-03-14T11:53:34Z", "type": "feedback", "id": "ilrxlswdomq2h2", "anon": "no", "bu